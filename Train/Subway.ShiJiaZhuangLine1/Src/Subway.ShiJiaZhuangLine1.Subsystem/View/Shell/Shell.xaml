<UserControl
    x:Class="Subway.ShiJiaZhuangLine1.Subsystem.View.Shell.Shell"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:constant="clr-namespace:Subway.ShiJiaZhuangLine1.Subsystem.Constant"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Subway.ShiJiaZhuangLine1.Subsystem.View.Shell"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:regions="http://www.codeplex.com/CompositeWPF"
    d:DesignHeight="600"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <Viewbox Stretch="Fill">
        <ContentControl
            Width="800"
            Height="600"
            regions:RegionManager.RegionName="{x:Static constant:RegionNames.RootRegion}" />
    </Viewbox>
</UserControl>
