            <TextBlock Grid.Row="1" Grid.Column="1" Text="断开" Style="{StaticResource HelpViewTextStyle}"></TextBlock>
            <TextBlock Grid.Row="2" Grid.Column="1" Text="闭合" Style="{StaticResource HelpViewTextStyle}"></TextBlock>
            <TextBlock Grid.Row="3" Grid.Column="1" Text="断开" Style="{StaticResource HelpViewTextStyle}"></TextBlock>
