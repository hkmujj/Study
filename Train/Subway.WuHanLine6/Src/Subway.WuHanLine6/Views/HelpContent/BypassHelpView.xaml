            <TextBlock Grid.Row="11" Grid.Column="1" Text="断开" Style="{StaticResource HelpViewTextStyle}"></TextBlock>
            <TextBlock Grid.Row="12" Grid.Column="1" Text="闭合" Style="{StaticResource HelpViewTextStyle}"></TextBlock>
            <TextBlock Grid.Row="13" Grid.Column="1" Text="断开" Style="{StaticResource HelpViewTextStyle}"></TextBlock>
