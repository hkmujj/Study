            <Border Grid.Row="3" Grid.Column="2" Background="White" Margin="20,1,20,2"></Border>
            <Border Grid.Row="4" Grid.Column="2" Background="Yellow" Margin="20,1,20,2"></Border>
            <Border Grid.Row="5" Grid.Column="2" Background="White" Margin="20,1,20,2"></Border>
            <Border Grid.Row="6" Grid.Column="2" Background="Yellow" Margin="20,1,20,2"></Border>
            <Border Grid.Row="7" Grid.Column="2" Background="White" Margin="20,1,20,2"></Border>
            <Border Grid.Row="8" Grid.Column="2" Background="Yellow" Margin="20,1,20,2"></Border>
