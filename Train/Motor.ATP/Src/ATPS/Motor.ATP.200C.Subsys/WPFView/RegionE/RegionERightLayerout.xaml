<UserControl x:Class="Motor.ATP._200C.Subsys.WPFView.RegionE.RegionERightLayerout"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:constant="clr-namespace:Motor.ATP._200C.Subsys.Constant"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:regions="http://www.codeplex.com/CompositeWPF"
             d:DesignHeight="180"
             d:DesignWidth="224"
             mc:Ignorable="d">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="50*" />
            <RowDefinition Height="50*" />
            <RowDefinition Height="80*" />
        </Grid.RowDefinitions>
        <Border Grid.Row="0"
                Style="{StaticResource Motor.ATP.200C.BackGridBorderStyleLeftUp}"
                regions:RegionManager.RegionName="{x:Static constant:RegionNames.RegionEStationInfo}" />
        <Grid Grid.Row="1">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="61*" />
                <ColumnDefinition Width="61*" />
                <ColumnDefinition Width="61*" />
                <ColumnDefinition Width="61*" />
                <ColumnDefinition Width="61*" />
            </Grid.ColumnDefinitions>
            <Border Grid.Column="0"
                    Style="{StaticResource Motor.ATP.200C.BackGridBorderStyleLeftUp}"
                    regions:RegionManager.RegionName="{x:Static constant:RegionNames.RegionE11}" />
            <Border Grid.Column="1"
                    Style="{StaticResource Motor.ATP.200C.BackGridBorderStyleLeftUp}"
                    regions:RegionManager.RegionName="{x:Static constant:RegionNames.RegionE12}" />
            <Border Grid.Column="2"
                    Style="{StaticResource Motor.ATP.200C.BackGridBorderStyleLeftUp}"
                    regions:RegionManager.RegionName="{x:Static constant:RegionNames.RegionE13}" />
            <Border Grid.Column="3"
                    Style="{StaticResource Motor.ATP.200C.BackGridBorderStyleLeftUp}"
                    regions:RegionManager.RegionName="{x:Static constant:RegionNames.RegionE14}" />
            <Border Grid.Column="4"
                    Style="{StaticResource Motor.ATP.200C.BackGridBorderStyleLeftUp}"
                    regions:RegionManager.RegionName="{x:Static constant:RegionNames.RegionE15}" />
        </Grid>
        <Grid Grid.Row="2">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="80*" />
                <ColumnDefinition Width="114*" />
                <ColumnDefinition Width="80*" />
            </Grid.ColumnDefinitions>
            <Border Grid.Column="0"
                    Style="{StaticResource Motor.ATP.200C.BackGridBorderStyleLeftUp}"
                    regions:RegionManager.RegionName="{x:Static constant:RegionNames.RegionE16A}" />
            <Grid Grid.Column="1"
                  d:DesignHeight="80"
                  d:DesignWidth="114">
                <Grid.RowDefinitions>
                    <RowDefinition Height="40*" />
                    <RowDefinition Height="40*" />
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="57*" />
                    <ColumnDefinition Width="57*" />
                </Grid.ColumnDefinitions>
                <Border Grid.Row="0"
                        Grid.Column="0"
                        Style="{StaticResource Motor.ATP.200C.BackGridBorderStyleLeftUp}"
                        regions:RegionManager.RegionName="{x:Static constant:RegionNames.RegionE16B1}" />
                <Border Grid.Row="0"
                        Grid.Column="1"
                        Style="{StaticResource Motor.ATP.200C.BackGridBorderStyleUp}"
                        regions:RegionManager.RegionName="{x:Static constant:RegionNames.RegionE16B2}" />
                <Border Grid.Row="1"
                        Grid.Column="0"
                        Style="{StaticResource Motor.ATP.200C.BackGridBorderStyleLeftUp}"
                        regions:RegionManager.RegionName="{x:Static constant:RegionNames.RegionE16C}" />
                <Border Grid.Row="1"
                        Grid.Column="1"
                        Style="{StaticResource Motor.ATP.200C.BackGridBorderStyleLeftUp}"
                        regions:RegionManager.RegionName="{x:Static constant:RegionNames.RegionE16D}" />
            </Grid>
            <Border Grid.Column="2"
                    Style="{StaticResource Motor.ATP.200C.BackGridBorderStyleLeftUp}"
                    regions:RegionManager.RegionName="{x:Static constant:RegionNames.RegionE17}" />
        </Grid>
    </Grid>
</UserControl>
