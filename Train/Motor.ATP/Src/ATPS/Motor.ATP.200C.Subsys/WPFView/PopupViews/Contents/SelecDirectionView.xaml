<UserControl x:Class="Motor.ATP._200C.Subsys.WPFView.PopupViews.Contents.SelecDirectionView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Motor.ATP._200C.Subsys.WPFView.PopupViews.Contents"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             d:DesignHeight="250"
             d:DesignWidth="232"
             Background="{StaticResource Motor.ATP.200C.Bursh.PopViewContent}"
             mc:Ignorable="d">
    <Grid Margin="50,30">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="10" />
            <RowDefinition Height="Auto" />
            <RowDefinition />
        </Grid.RowDefinitions>
        <TextBlock Style="{StaticResource Motor.ATP.200C.PopupViewCenterContentTextStyle}" Text="{StaticResource Motor.ATP.Infrasturcture.String.DirectionUp}" />
        <TextBlock Grid.Row="2"
                   Style="{StaticResource Motor.ATP.200C.PopupViewCenterContentTextStyle}"
                   Text="{StaticResource Motor.ATP.Infrasturcture.String.DirectionDown}" />
    </Grid>
</UserControl>
