<UserControl x:Class="Motor.ATP._300T.Subsys.WPFView.PopupViews.Contents.SelecDirectionView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Motor.ATP._300T.Subsys.WPFView.PopupViews.Contents"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:regionf="clr-namespace:Motor.ATP._300T.Subsys.WPFView.RegionF"
             Background="{StaticResource Motor.ATP.300T.PopviewBackgroudBrush}"
             d:DataContext="{d:DesignInstance regionf:RegionFText}"
             d:DesignHeight="250"
             d:DesignWidth="232"
             mc:Ignorable="d">
    <Grid Margin="50">
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition />
        </Grid.RowDefinitions>
        <Grid>
            <TextBlock Style="{StaticResource Motor.ATP.300T.PopupViewContentTextStyle}" Text="{StaticResource Motor.ATP.Infrasturcture.String.DirectionUp}" />
            <Rectangle Name="Rectangle1"
                       Margin="0,25,35,15"
                       Stretch="Fill"
                       Stroke="{StaticResource Motor.ATP.300T.WhiteBrush}"
                       StrokeThickness="2"
                       Visibility="Hidden" />
        </Grid>
        <Grid Grid.Row="1">
            <TextBlock Style="{StaticResource Motor.ATP.300T.PopupViewContentTextStyle}" Text="{StaticResource Motor.ATP.Infrasturcture.String.DirectionDown}" />
            <Rectangle Name="Rectangle2"
                       Margin="0,25,35,15"
                       Stretch="Fill"
                       Stroke="{StaticResource Motor.ATP.300T.WhiteBrush}"
                       StrokeThickness="2"
                       Visibility="Hidden" />
        </Grid>
        <!--<TextBlock Style="{StaticResource Motor.ATP.300T.PopupViewContentTextStyle}" Text="{StaticResource Motor.ATP.Infrasturcture.String.DirectionUp}" />-->
        <!--<TextBlock Grid.Row="1"
                   Style="{StaticResource Motor.ATP.300T.PopupViewContentTextStyle}"
                   Text="{StaticResource Motor.ATP.Infrasturcture.String.DirectionDown}" />-->
    </Grid>
</UserControl>
