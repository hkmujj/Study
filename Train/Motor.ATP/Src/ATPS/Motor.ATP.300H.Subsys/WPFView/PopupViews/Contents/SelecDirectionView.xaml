<UserControl x:Class="Motor.ATP._300H.Subsys.WPFView.PopupViews.Contents.SelecDirectionView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Motor.ATP._300H.Subsys.WPFView.PopupViews.Contents"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             d:DesignHeight="250"
             d:DesignWidth="232"
             Background="{StaticResource Motor.ATP.300H.BackgroudBrush}"
             mc:Ignorable="d">
    <Grid Margin="50">
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition />
        </Grid.RowDefinitions>
        <TextBlock Style="{StaticResource Motor.ATP.300H.PopupViewContentTextStyle}" Text="{StaticResource Motor.ATP.Infrasturcture.String.DirectionUp}" />
        <TextBlock Grid.Row="1"
                   Style="{StaticResource Motor.ATP.300H.PopupViewContentTextStyle}"
                   Text="{StaticResource Motor.ATP.Infrasturcture.String.DirectionDown}" />
    </Grid>
</UserControl>
