<UserControl x:Class="Motor.ATP._300H.Subsys.WPFView.RegionC.RegionCLayerout"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:constant="clr-namespace:Motor.ATP._300H.Subsys.Constant"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:regions="http://www.codeplex.com/CompositeWPF"
             d:DesignHeight="54"
             d:DesignWidth="334"
             mc:Ignorable="d">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="54*" />

            <ColumnDefinition Width="35*" />
            <ColumnDefinition Width="35*" />
            <ColumnDefinition Width="35*" />

            <ColumnDefinition Width="70*" />

            <ColumnDefinition Width="35*" />
            <ColumnDefinition Width="35*" />
            <ColumnDefinition Width="35*" />
        </Grid.ColumnDefinitions>
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="27*" />
                <RowDefinition Height="27*" />
            </Grid.RowDefinitions>
            <Border Grid.Row="0"
                    Style="{StaticResource Motor.ATP.300H.BackGridBorderStyleLeftUp}"
                    regions:RegionManager.RegionName="{x:Static constant:RegionNames.RegionC08}" />
            <Border Grid.Row="1"
                    Style="{StaticResource Motor.ATP.300H.BackGridBorderStyleLeftUp}"
                    regions:RegionManager.RegionName="{x:Static constant:RegionNames.RegionC09}" />
        </Grid>
        <Border Grid.Column="1"
                Style="{StaticResource Motor.ATP.300H.BackGridBorderStyleLeftUp}"
                regions:RegionManager.RegionName="{x:Static constant:RegionNames.RegionC05}" />
        <Border Grid.Column="2"
                Style="{StaticResource Motor.ATP.300H.BackGridBorderStyleLeftUp}"
                regions:RegionManager.RegionName="{x:Static constant:RegionNames.RegionC06}" />
        <Border Grid.Column="3"
                Style="{StaticResource Motor.ATP.300H.BackGridBorderStyleLeftUp}"
                regions:RegionManager.RegionName="{x:Static constant:RegionNames.RegionC07}" />
        <Border Grid.Column="4"
                Style="{StaticResource Motor.ATP.300H.BackGridBorderStyleLeftUp}"
                regions:RegionManager.RegionName="{x:Static constant:RegionNames.RegionC01}" />
        <Border Grid.Column="5"
                Style="{StaticResource Motor.ATP.300H.BackGridBorderStyleLeftUp}"
                regions:RegionManager.RegionName="{x:Static constant:RegionNames.RegionC02}" />
        <Border Grid.Column="6"
                Style="{StaticResource Motor.ATP.300H.BackGridBorderStyleLeftUp}"
                regions:RegionManager.RegionName="{x:Static constant:RegionNames.RegionC03}" />
        <Border Grid.Column="7"
                Style="{StaticResource Motor.ATP.300H.BackGridBorderStyleLeftUp}"
                regions:RegionManager.RegionName="{x:Static constant:RegionNames.RegionC04}" />
    </Grid>
</UserControl>
