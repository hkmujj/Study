<UserControl x:Class="Motor.ATP._300H.Subsys.WPFView.RegionF.RegionFLayerout"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:interface="clr-namespace:Motor.ATP.Infrasturcture.Interface;assembly=Motor.ATP.Infrasturcture"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:regionF="clr-namespace:Motor.ATP._300H.Subsys.WPFView.RegionF"
             d:DataContext="{d:DesignInstance interface:IATP}"
             d:DesignHeight="480"
             d:DesignWidth="62"
             mc:Ignorable="d">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
        </Grid.RowDefinitions>
        <Border Grid.Row="0" Style="{StaticResource Motor.ATP.300H.BackGridBorderStyleLeftUp}">
            <regionF:RegionFText Grid.Row="0"
                                 IsEnabled="{Binding Path=DriverInterfaceController.CurrentInterface.DriverSelectable[F1].Enabled}"
                                 Text="{Binding Path=DriverInterfaceController.CurrentInterface.DriverSelectable[F1].Content}" />
        </Border>
        <Border Grid.Row="1" Style="{StaticResource Motor.ATP.300H.BackGridBorderStyleLeftUp}">
            <regionF:RegionFText Grid.Row="0"
                                 IsEnabled="{Binding Path=DriverInterfaceController.CurrentInterface.DriverSelectable[F2].Enabled}"
                                 Text="{Binding Path=DriverInterfaceController.CurrentInterface.DriverSelectable[F2].Content}" />
        </Border>
        <Border Grid.Row="2" Style="{StaticResource Motor.ATP.300H.BackGridBorderStyleLeftUp}">
            <regionF:RegionFText Grid.Row="0"
                                 IsEnabled="{Binding Path=DriverInterfaceController.CurrentInterface.DriverSelectable[F3].Enabled}"
                                 Text="{Binding Path=DriverInterfaceController.CurrentInterface.DriverSelectable[F3].Content}" />
        </Border>
        <Border Grid.Row="3" Style="{StaticResource Motor.ATP.300H.BackGridBorderStyleLeftUp}">
            <regionF:RegionFText Grid.Row="0"
                                 IsEnabled="{Binding Path=DriverInterfaceController.CurrentInterface.DriverSelectable[F4].Enabled}"
                                 Text="{Binding Path=DriverInterfaceController.CurrentInterface.DriverSelectable[F4].Content}" />
        </Border>
        <Border Grid.Row="4" Style="{StaticResource Motor.ATP.300H.BackGridBorderStyleLeftUp}">
            <regionF:RegionFText Grid.Row="0"
                                 IsEnabled="{Binding Path=DriverInterfaceController.CurrentInterface.DriverSelectable[F5].Enabled}"
                                 Text="{Binding Path=DriverInterfaceController.CurrentInterface.DriverSelectable[F5].Content}" />
        </Border>
        <Border Grid.Row="5" Style="{StaticResource Motor.ATP.300H.BackGridBorderStyleLeftUp}">
            <regionF:RegionFText Grid.Row="0"
                                 IsEnabled="{Binding Path=DriverInterfaceController.CurrentInterface.DriverSelectable[F6].Enabled}"
                                 Text="{Binding Path=DriverInterfaceController.CurrentInterface.DriverSelectable[F6].Content}" />
        </Border>
        <Border Grid.Row="6" Style="{StaticResource Motor.ATP.300H.BackGridBorderStyleLeftUp}">
            <regionF:RegionFText Grid.Row="0"
                                 IsEnabled="{Binding Path=DriverInterfaceController.CurrentInterface.DriverSelectable[F7].Enabled}"
                                 Text="{Binding Path=DriverInterfaceController.CurrentInterface.DriverSelectable[F7].Content}" />
        </Border>
        <Border Grid.Row="7" Style="{StaticResource Motor.ATP.300H.BackGridBorderStyleLeftUp}">
            <regionF:RegionFText Grid.Row="0"
                                 IsEnabled="{Binding Path=DriverInterfaceController.CurrentInterface.DriverSelectable[F8].Enabled}"
                                 Text="{Binding Path=DriverInterfaceController.CurrentInterface.DriverSelectable[F8].Content}" />
        </Border>
    </Grid>
</UserControl>
