<UserControl
    x:Class="Engine.TCMS.Turkmenistan.View.Contents.RunParam.RunParamPage3"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:common="clr-namespace:Engine.TCMS.Turkmenistan.View.Common"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:viewModel="clr-namespace:Engine.TCMS.Turkmenistan.ViewModel"
    d:DataContext="{d:DesignInstance viewModel:DomainViewModel}"
    d:DesignHeight="600"
    d:DesignWidth="245"
    mc:Ignorable="d">
    <Grid>
        <Border Style="{DynamicResource MainBorderStyle}">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="70*" />
                    <RowDefinition Height="480*" />
                </Grid.RowDefinitions>
                <Grid Grid.Row="0" Margin="2,5">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition />
                        <ColumnDefinition />
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition />
                        <RowDefinition />
                    </Grid.RowDefinitions>
                    <Button
                        Grid.Row="0"
                        Grid.Column="0"
                        Content="{DynamicResource EngineSpeed}"
                        Style="{DynamicResource MainTextButtonStyle6}" />
                    <Button
                        Grid.Row="1"
                        Grid.Column="0"
                        Content="{DynamicResource DieselEngineSpeed}"
                        Style="{DynamicResource MainTextButtonStyle6}" />
                    <common:EffectBorder
                        Grid.Row="0"
                        Grid.RowSpan="2"
                        Grid.Column="1"
                        Background="{DynamicResource BackgroudBrush}"
                        Style="{DynamicResource MainViewBorderStyle2}">

                        <common:EffectBorder.EffectContent>
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition />
                                    <RowDefinition />
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="1*" />
                                    <ColumnDefinition Width="1*" />
                                </Grid.ColumnDefinitions>
                                <TextBlock
                                    Grid.Row="0"
                                    Grid.Column="0"
                                    Style="{DynamicResource MainTextStyle3}"
                                    Text="{Binding Model.MainModel.EngineSpeed, StringFormat={}{0:F0}}" />
                                <TextBlock
                                    Grid.Row="1"
                                    Grid.Column="0"
                                    Style="{DynamicResource MainTextStyle3}"
                                    Text="{Binding Model.MainModel.DieselEngineSpeed, StringFormat={}{0:F0}}" />
                                <TextBlock
                                    Grid.Row="0"
                                    Grid.Column="1"
                                    Style="{DynamicResource MainTextStyle4}"
                                    Text="km/h" />
                                <TextBlock
                                    Grid.Row="1"
                                    Grid.Column="1"
                                    Style="{DynamicResource MainTextStyle4}"
                                    Text="r/min" />
                            </Grid>
                        </common:EffectBorder.EffectContent>
                    </common:EffectBorder>
                </Grid>
                <Border
                    Grid.Row="1"
                    Margin="5"
                    Background="{DynamicResource BackgroudBrush}"
                    BorderBrush="DarkGray"
                    BorderThickness="2">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="40" />
                            <RowDefinition />
                        </Grid.RowDefinitions>
                        <Button Content="{DynamicResource RunParam}" Style="{DynamicResource MainTextButtonStyle5}" />
                        <Grid Grid.Row="1" Margin="10,10,0,35">
                            <Grid.RowDefinitions>
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                            </Grid.RowDefinitions>
                            <Grid Grid.Row="0">
                                <Button Content="{DynamicResource ElectricCurrent1D}" Style="{DynamicResource RunParamButtonTextStyle1}" />
                                <Button Content="{Binding Model.RunParamModel.ElectricCurrent1D}" Style="{DynamicResource RunParamButtonTextStyle8}" />
                                <Button Content="A" Style="{DynamicResource RunParamButtonTextStyle3}" />
                            </Grid>
                            <Grid Grid.Row="1">
                                <Button Content="{DynamicResource ElectricCurrent2D}" Style="{DynamicResource RunParamButtonTextStyle1}" />
                                <Button Content="{Binding Model.RunParamModel.ElectricCurrent2D}" Style="{DynamicResource RunParamButtonTextStyle8}" />
                                <Button Content="A" Style="{DynamicResource RunParamButtonTextStyle3}" />
                            </Grid>
                            <Grid Grid.Row="2">
                                <Button Content="{DynamicResource ElectricCurrent3D}" Style="{DynamicResource RunParamButtonTextStyle1}" />
                                <Button Content="{Binding Model.RunParamModel.ElectricCurrent3D}" Style="{DynamicResource RunParamButtonTextStyle8}" />
                                <Button Content="A" Style="{DynamicResource RunParamButtonTextStyle3}" />
                            </Grid>
                            <Grid Grid.Row="3">
                                <Button Content="{DynamicResource ElectricCurrent4D}" Style="{DynamicResource RunParamButtonTextStyle1}" />
                                <Button Content="{Binding Model.RunParamModel.ElectricCurrent4D}" Style="{DynamicResource RunParamButtonTextStyle8}" />
                                <Button Content="A" Style="{DynamicResource RunParamButtonTextStyle3}" />
                            </Grid>
                            <Grid Grid.Row="4">
                                <Button Content="{DynamicResource ElectricCurrent5D}" Style="{DynamicResource RunParamButtonTextStyle1}" />
                                <Button Content="{Binding Model.RunParamModel.ElectricCurrent5D}" Style="{DynamicResource RunParamButtonTextStyle8}" />
                                <Button Content="A" Style="{DynamicResource RunParamButtonTextStyle3}" />
                            </Grid>
                            <Grid Grid.Row="5">
                                <Button Content="{DynamicResource ElectricCurrent6D}" Style="{DynamicResource RunParamButtonTextStyle1}" />
                                <Button Content="{Binding Model.RunParamModel.ElectricCurrent6D}" Style="{DynamicResource RunParamButtonTextStyle8}" />
                                <Button Content="A" Style="{DynamicResource RunParamButtonTextStyle3}" />
                            </Grid>
                            <Grid Grid.Row="6">
                                <Button Content="{DynamicResource MotorCurrentDifference}" Style="{DynamicResource RunParamButtonTextStyle1}" />
                                <Button Content="{Binding Model.RunParamModel.MotorCurrentDifference}" Style="{DynamicResource RunParamButtonTextStyle8}" />
                                <Button Content="A" Style="{DynamicResource RunParamButtonTextStyle3}" />
                            </Grid>
                            <Grid Grid.Row="7">
                                <Button Content="{DynamicResource CurrentDistributionFactor}" Style="{DynamicResource RunParamButtonTextStyle1}" />
                                <Button Content="{Binding Model.RunParamModel.CurrentDistributionFactor}" Style="{DynamicResource RunParamButtonTextStyle8}" />
                                <Button Content="%" Style="{DynamicResource RunParamButtonTextStyle3}" />
                            </Grid>
                            <Grid Grid.Row="8">
                                <Button Content="{DynamicResource Speed1D}" Style="{DynamicResource RunParamButtonTextStyle1}" />
                                <Button Content="{Binding Model.RunParamModel.Speed1D}" Style="{DynamicResource RunParamButtonTextStyle8}" />
                                <Button Content="r/min" Style="{DynamicResource RunParamButtonTextStyle3}" />
                            </Grid>
                            <Grid Grid.Row="9">
                                <Button Content="{DynamicResource Speed2D}" Style="{DynamicResource RunParamButtonTextStyle1}" />
                                <Button Content="{Binding Model.RunParamModel.Speed2D}" Style="{DynamicResource RunParamButtonTextStyle8}" />
                                <Button Content="r/min" Style="{DynamicResource RunParamButtonTextStyle3}" />
                            </Grid>
                            <Grid Grid.Row="10">
                                <Button Content="{DynamicResource Speed3D}" Style="{DynamicResource RunParamButtonTextStyle1}" />
                                <Button Content="{Binding Model.RunParamModel.Speed3D}" Style="{DynamicResource RunParamButtonTextStyle8}" />
                                <Button Content="r/min" Style="{DynamicResource RunParamButtonTextStyle3}" />
                            </Grid>
                            <Grid Grid.Row="11">
                                <Button Content="{DynamicResource Speed4D}" Style="{DynamicResource RunParamButtonTextStyle1}" />
                                <Button Content="{Binding Model.RunParamModel.Speed4D}" Style="{DynamicResource RunParamButtonTextStyle8}" />
                                <Button Content="r/min" Style="{DynamicResource RunParamButtonTextStyle3}" />
                            </Grid>
                            <Grid Grid.Row="12">
                                <Button Content="{DynamicResource Speed5D}" Style="{DynamicResource RunParamButtonTextStyle1}" />
                                <Button Content="{Binding Model.RunParamModel.Speed5D}" Style="{DynamicResource RunParamButtonTextStyle8}" />
                                <Button Content="r/min" Style="{DynamicResource RunParamButtonTextStyle3}" />
                            </Grid>
                            <Grid Grid.Row="13">
                                <Button Content="{DynamicResource Speed6D}" Style="{DynamicResource RunParamButtonTextStyle1}" />
                                <Button Content="{Binding Model.RunParamModel.Speed6D}" Style="{DynamicResource RunParamButtonTextStyle8}" />
                                <Button Content="r/min" Style="{DynamicResource RunParamButtonTextStyle3}" />
                            </Grid>
                            <Grid Grid.Row="14">
                                <Button Content="{DynamicResource MotorSpeedDifference}" Style="{DynamicResource RunParamButtonTextStyle1}" />
                                <Button Content="{Binding Model.RunParamModel.MotorSpeedDifference}" Style="{DynamicResource RunParamButtonTextStyle8}" />
                                <Button Content="r/min" Style="{DynamicResource RunParamButtonTextStyle3}" />
                            </Grid>
                        </Grid>
                    </Grid>
                </Border>


            </Grid>
        </Border>

    </Grid>
</UserControl>
