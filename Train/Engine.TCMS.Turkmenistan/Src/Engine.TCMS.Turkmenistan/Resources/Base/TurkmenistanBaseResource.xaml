<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="../Converters/TurkmenistanConverter.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <SolidColorBrush x:Key="ContentBackgroundBrush" Color="#FF7F7E81" />

    <SolidColorBrush x:Key="ShellBackgroundBrush" Color="Black" />

    <SolidColorBrush x:Key="ForegroundWhiteBrush" Color="White" />

    <SolidColorBrush x:Key="ForegroundOrangeBrush" Color="Orange" />

    <SolidColorBrush x:Key="MainTextBrushOne" Color="PapayaWhip" />

    <SolidColorBrush x:Key="ContentDeepSkyBlueBrush" Color="DeepSkyBlue" />

    <SolidColorBrush x:Key="BackgroudBrush" Color="#1A2A3A" />

    <SolidColorBrush x:Key="HarwareButtonWhiteBrush" Color="White" />

    <SolidColorBrush x:Key="BlackBrush" Color="Black" />


    <SolidColorBrush x:Key="HarwareButtonRedBrush" Color="Red" />

    <SolidColorBrush x:Key="HarwareButtonPressedBackground" Color="Black" />

    <system:Double x:Key="Double30">30</system:Double>

    <system:Double x:Key="Double20">20</system:Double>

    <system:Double x:Key="Double18">18</system:Double>

    <system:Double x:Key="Double16">16</system:Double>
    <system:Double x:Key="Double14">14</system:Double>
    <system:Double x:Key="Double12">12</system:Double>

    <system:String x:Key="OilPressureParamFormat" xml:space="preserve">滑油压力      {0}    Kpa</system:String>
    <system:String x:Key="FuelPressureParamFormat" xml:space="preserve">燃油压力      {0}    Kpa</system:String>
    <system:String x:Key="CoolingWaterTemperatureParamFormat" xml:space="preserve">冷却水温     {0}    ℃</system:String>
    <system:String x:Key="OILTEngineOilTemperatureParamFormat" xml:space="preserve">机油温度     {0}    ℃</system:String>
    <system:String x:Key="PowerTemperatureParamFormat1" xml:space="preserve">功率环温1    {0}    ℃</system:String>
    <system:String x:Key="PowerTemperatureParamFormat2" xml:space="preserve">功率环温2    {0}    ℃</system:String>
    <system:String x:Key="BrakePressParamFormat" xml:space="preserve">制动风缸压力  {0}    Kpa</system:String>

</ResourceDictionary>