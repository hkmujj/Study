<UserControl x:Class="Engine.TCMS.HXD3.View.Contents.Overhaul.States.ActionNumberView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Engine.TCMS.HXD3.View.Contents"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:viewModel="clr-namespace:Engine.TCMS.HXD3.ViewModel"
             d:DataContext="{d:DesignInstance viewModel:HXD3TCMSViewModel}"
             d:DesignHeight="438"
             d:DesignWidth="798"
             Background="{StaticResource ContentBackgroudBrush}"
             mc:Ignorable="d">
    <Grid>
        <ItemsControl Margin="50,50,80,100"
                      ItemsSource="{Binding TCMS.ActionCountViewModel.NumUnits}"
                      Style="{StaticResource ActionNumItemStyle}" />
    </Grid>
</UserControl>
