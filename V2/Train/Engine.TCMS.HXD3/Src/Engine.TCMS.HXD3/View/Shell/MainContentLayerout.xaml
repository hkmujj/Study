<UserControl x:Class="Engine.TCMS.HXD3.View.Shell.MainContentLayerout"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:constant="clr-namespace:Engine.TCMS.HXD3.Constant"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:regions="http://www.codeplex.com/CompositeWPF"
             d:DesignHeight="520"
             d:DesignWidth="800"
             mc:Ignorable="d">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="70*" />
            <RowDefinition Height="450*" />
        </Grid.RowDefinitions>
        <Border Grid.Row="0" regions:RegionManager.RegionName="{x:Static constant:RegionNames.RegionMainContentTitle}" />
        <Border Grid.Row="1"
                Margin="1,6"
                regions:RegionManager.RegionName="{x:Static constant:RegionNames.RegionMainContentContent}" />
    </Grid>
</UserControl>
