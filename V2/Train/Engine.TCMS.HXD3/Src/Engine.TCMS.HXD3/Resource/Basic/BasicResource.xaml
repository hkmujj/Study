<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <SolidColorBrush x:Key="WhiteBrush" Color="White" />
    <SolidColorBrush x:Key="BlackBrush" Color="Black" />
    <SolidColorBrush x:Key="YellowBrush" Color="Yellow" />
    <SolidColorBrush x:Key="GreenBrush" Color="LimeGreen" />
    <SolidColorBrush x:Key="DeepSkyBlueBrush" Color="DeepSkyBlue" />
    <SolidColorBrush x:Key="BlueBrush" Color="Blue" />
    <SolidColorBrush x:Key="VioletBrush" Color="Violet" />
    <SolidColorBrush x:Key="RedBrush" Color="Red" />
    <SolidColorBrush x:Key="TransparentBrush" Color="Transparent" />


    <SolidColorBrush x:Key="ContentBackgroudBrush" Color="Black" />
    <SolidColorBrush x:Key="TitleBackgroudBrush" Color="Blue" />

    <SolidColorBrush x:Key="PasswordSetteingBack" Color="AntiqueWhite" />

    <SolidColorBrush x:Key="GridLineForegroundBrush" Color="White" />

    <SolidColorBrush x:Key="BtnLineBrush" Color="White" />

    <SolidColorBrush x:Key="TextForegroundBrush" Color="White" />

    <SolidColorBrush x:Key="LowLightBrush" Color="#5F000000" />
    <SolidColorBrush x:Key="MidLightBrush" Color="#1F000000" />
    <SolidColorBrush x:Key="HighLightBrush" Color="#00000000" />

    <FontFamily x:Key="LEDFontFamily">/Engine.TCMS.HXD3;component/Resource/Fonts/#DigifaceWide</FontFamily>

    <system:Boolean x:Key="BoolTrue">True</system:Boolean>
    <system:Boolean x:Key="BoolFalse">False</system:Boolean>

    <Visibility x:Key="Visible">Visible</Visibility>
    <Visibility x:Key="Hidden">Hidden</Visibility>

</ResourceDictionary>