<UserControl x:Class="Engine.TPX21F.HXN5B.View.Contents.LowConstSpeed.LowConstSpeedView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:common="clr-namespace:Engine.TPX21F.HXN5B.View.Common"
             xmlns:constant="clr-namespace:Engine.TPX21F.HXN5B.Constant"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:regions="http://www.codeplex.com/CompositeWPF"
             xmlns:view="clr-namespace:MMI.Facility.WPFInfrastructure.View;assembly=MMI.Facility.WPFInfrastructure"
             xmlns:viewModel="clr-namespace:Engine.TPX21F.HXN5B.ViewModel"
             Background="{StaticResource ContentBackgroundBrush}"
             d:DataContext="{d:DesignInstance viewModel:HXN5BViewModel}"
             d:DesignHeight="495"
             d:DesignWidth="800"
             mc:Ignorable="d">
    <GroupBox Margin="80,94,76,57" Style="{StaticResource SettingViewGroupBoxStyle}">
        <GroupBox.Header>
            <StackPanel Orientation="Horizontal">
                <TextBlock Style="{StaticResource SettingLowSpeedContentTextStyle}" Text="{StaticResource StringLowSpeed}" />
                <TextBlock Style="{StaticResource SettingLowSpeedContentTextStyle}" Text="{StaticResource StringControl}" />
            </StackPanel>
        </GroupBox.Header>
        <StackPanel Margin="10,50"
                    HorizontalAlignment="Center"
                    Orientation="Vertical">
            <StackPanel Orientation="Horizontal">
                <TextBlock Style="{StaticResource SettingLowSpeedContentTextStyle}" Text="{StaticResource StringLowSpeed}" />
                <TextBlock Style="{StaticResource SettingLowSpeedContentTextStyle}" Text="{StaticResource StringControl}" />
                <TextBlock Style="{StaticResource SettingLowSpeedContentTextStyle}" Text="{StaticResource StringSwitch}" />
            </StackPanel>
            <TextBlock Style="{StaticResource SettingLowSpeedContentBlueTextStyle}" Text="{StaticResource StringPutInto}" />
            <common:SoftSwtichItemView Width="80" Height="80" />
            <TextBlock Style="{StaticResource SettingLowSpeedContentBlueTextStyle}" Text="{StaticResource StringCutoff}" />
        </StackPanel>
    </GroupBox>
</UserControl>
