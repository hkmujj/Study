<UserControl x:Class="Engine.TPX21F.HXN5B.View.Contents.Other.SoftVersionView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:common="clr-namespace:Engine.TPX21F.HXN5B.View.Common"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:viewModel="clr-namespace:Engine.TPX21F.HXN5B.ViewModel"
             Background="{StaticResource ContentBackgroundBrush}"
             d:DataContext="{d:DesignInstance viewModel:HXN5BViewModel}"
             d:DesignHeight="495"
             d:DesignWidth="800"
             mc:Ignorable="d">
    <GroupBox Margin="40,40,40,10"
              Style="{StaticResource CenterHeaderGroupBoxStyle}"
              common:GroupBoxHeader.HeaderWidth="{Binding ElementName=HeaderPanel,
                                                          Path=ActualWidth,
                                                          Converter={StaticResource MathConverter},
                                                          ConverterParameter=x+120}">
        <GroupBox.Header>
            <StackPanel x:Name="HeaderPanel"
                        Orientation="Horizontal"
                        x:FieldModifier="private">
                <TextBlock Style="{StaticResource ContentConstWhiteTextStyle}" Text="{StaticResource StringEngine}" />
                <TextBlock Style="{StaticResource ContentConstWhiteTextStyle}" Text="{StaticResource StringSoft}" />
                <TextBlock Style="{StaticResource ContentConstWhiteTextStyle}" Text="{StaticResource StringConfigure}" />
            </StackPanel>
        </GroupBox.Header>

        <Grid Margin="30,10">
            <Grid.Resources>
                <Style BasedOn="{StaticResource ContentConstGreenTextStyle}" TargetType="TextBlock">
                    <Setter Property="TextAlignment" Value="Left" />
                </Style>
            </Grid.Resources>
            <Grid.RowDefinitions>
                <RowDefinition />
                <RowDefinition />
                <RowDefinition />
                <RowDefinition />
                <RowDefinition />
                <RowDefinition />
                <RowDefinition />
                <RowDefinition />
                <RowDefinition />
                <RowDefinition />
                <RowDefinition />
                <RowDefinition />
                <RowDefinition />
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="66*" />
                <ColumnDefinition Width="236*" />
                <ColumnDefinition Width="Auto" />
            </Grid.ColumnDefinitions>

            <TextBlock Text="车辆控制单元（LCS）" />
            <TextBlock Grid.Row="0"
                       Grid.Column="1"
                       Text="652.82"
                       TextAlignment="Center" />
            <TextBlock Grid.Row="0"
                       Grid.Column="3"
                       Text="AAC(DSP底板版本）" />
            <TextBlock Grid.Row="0"
                       Grid.Column="4"
                       Text="0.00"
                       TextAlignment="Center" />

            <TextBlock Grid.Row="1"
                       Grid.Column="0"
                       Text="智能显示单元（IDU）" />
            <TextBlock Grid.Row="1"
                       Grid.Column="1"
                       Text="0.0.0"
                       TextAlignment="Center" />
            <TextBlock Grid.Row="1"
                       Grid.Column="3"
                       Text="TAC(DSP底板版本）" />
            <TextBlock Grid.Row="1"
                       Grid.Column="4"
                       Text="0.00"
                       TextAlignment="Center" />

            <TextBlock Grid.Row="2"
                       Grid.Column="0"
                       Text="空压机控制器1（COMP1INV）" />
            <TextBlock Grid.Row="2"
                       Grid.Column="1"
                       Text="0.0"
                       TextAlignment="Center" />
            <TextBlock Grid.Row="2"
                       Grid.Column="3"
                       Text="BCC(DSP底板版本）" />
            <TextBlock Grid.Row="2"
                       Grid.Column="4"
                       Text="0.00"
                       TextAlignment="Center" />

            <TextBlock Grid.Row="3"
                       Grid.Column="0"
                       Text="空压机控制器2（COMP1INV）" />
            <TextBlock Grid.Row="3"
                       Grid.Column="1"
                       Text="0.0"
                       TextAlignment="Center" />
            <TextBlock Grid.Row="3"
                       Grid.Column="3"
                       Text="语音报警版本(AAP)" />
            <TextBlock Grid.Row="3"
                       Grid.Column="4"
                       Text="0.00"
                       TextAlignment="Center" />


            <TextBlock Grid.Row="4"
                       Grid.Column="0"
                       Text="牵引电机通风机控制器（TBC）" />
            <TextBlock Grid.Row="4"
                       Grid.Column="1"
                       Text="0.0"
                       TextAlignment="Center" />
            <TextBlock Grid.Row="4"
                       Grid.Column="3"
                       Text="燃油监控仪版本" />
            <TextBlock Grid.Row="4"
                       Grid.Column="4"
                       Text="0.00"
                       TextAlignment="Center" />

            <TextBlock Grid.Row="5"
                       Grid.Column="0"
                       Text="散热器风扇控制器1（RFC1INV）" />
            <TextBlock Grid.Row="5"
                       Grid.Column="1"
                       Text="0.0"
                       TextAlignment="Center" />
            <TextBlock Grid.Row="5"
                       Grid.Column="3"
                       Text="LCS底层软件版本" />
            <TextBlock Grid.Row="5"
                       Grid.Column="4"
                       Text="0.01"
                       TextAlignment="Center" />


            <TextBlock Grid.Row="6"
                       Grid.Column="0"
                       Text="散热器风扇控制器2（RFC1INV）" />
            <TextBlock Grid.Row="6"
                       Grid.Column="1"
                       Text="0.0"
                       TextAlignment="Center" />
            <TextBlock Grid.Row="6"
                       Grid.Column="3"
                       Text="柴油机软件版本" />
            <TextBlock Grid.Row="6"
                       Grid.Column="4"
                       Text="0.00"
                       TextAlignment="Center" />


            <TextBlock Grid.Row="7"
                       Grid.Column="0"
                       Text="一架SMC逻辑程序" />
            <TextBlock Grid.Row="7"
                       Grid.Column="1"
                       Text="0.00"
                       TextAlignment="Center" />
            <TextBlock Grid.Row="7"
                       Grid.Column="3"
                       Text="二架SMC逻辑程序" />
            <TextBlock Grid.Row="7"
                       Grid.Column="4"
                       Text="0.00"
                       TextAlignment="Center" />

            <TextBlock Grid.Row="8"
                       Grid.Column="0"
                       Text="一架MCC逆变程序" />
            <TextBlock Grid.Row="8"
                       Grid.Column="1"
                       Text="0.00"
                       TextAlignment="Center" />
            <TextBlock Grid.Row="8"
                       Grid.Column="3"
                       Text="二架MCC逆变程序" />
            <TextBlock Grid.Row="8"
                       Grid.Column="4"
                       Text="0.00"
                       TextAlignment="Center" />

            <TextBlock Grid.Row="9"
                       Grid.Column="0"
                       Text="一架MCC粘着程序" />
            <TextBlock Grid.Row="9"
                       Grid.Column="1"
                       Text="0.00"
                       TextAlignment="Center" />
            <TextBlock Grid.Row="9"
                       Grid.Column="3"
                       Text="二架MCC粘着程序" />
            <TextBlock Grid.Row="9"
                       Grid.Column="4"
                       Text="0.00"
                       TextAlignment="Center" />

            <TextBlock Grid.Row="10"
                       Grid.Column="0"
                       Text="GWMM硬件版本" />
            <TextBlock Grid.Row="10"
                       Grid.Column="1"
                       Text="0.0.0"
                       TextAlignment="Center" />
            <TextBlock Grid.Row="10"
                       Grid.Column="3"
                       Text="GWMS硬件版本" />
            <TextBlock Grid.Row="10"
                       Grid.Column="4"
                       Text="0.0.0"
                       TextAlignment="Center" />

            <TextBlock Grid.Row="11"
                       Grid.Column="0"
                       Text="GWMM固件版本" />
            <TextBlock Grid.Row="11"
                       Grid.Column="1"
                       Text="0.0.0"
                       TextAlignment="Center" />
            <TextBlock Grid.Row="11"
                       Grid.Column="3"
                       Text="GWMS固件版本" />
            <TextBlock Grid.Row="11"
                       Grid.Column="4"
                       Text="0.0.0"
                       TextAlignment="Center" />

            <TextBlock Grid.Row="12"
                       Grid.Column="0"
                       Text="GWMM软件版本" />
            <TextBlock Grid.Row="12"
                       Grid.Column="1"
                       Text="0.0.0"
                       TextAlignment="Center" />
            <TextBlock Grid.Row="12"
                       Grid.Column="3"
                       Text="GWMS软件版本" />
            <TextBlock Grid.Row="12"
                       Grid.Column="4"
                       Text="0.0.0"
                       TextAlignment="Center" />
        </Grid>
    </GroupBox>
</UserControl>
