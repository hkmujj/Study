<UserControl x:Class="Engine.TPX21F.HXN5B.View.Contents.Monitor.Detail.MonitorOilEnginStartContentView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:viewModel="clr-namespace:Engine.TPX21F.HXN5B.ViewModel"
             Background="{StaticResource ContentBackgroundBrush}"
             d:DataContext="{d:DesignInstance viewModel:HXN5BViewModel}"
             d:DesignHeight="275"
             d:DesignWidth="800"
             mc:Ignorable="d">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1.2*" />
            <ColumnDefinition Width="1.1*" />
            <ColumnDefinition />
        </Grid.ColumnDefinitions>

        <!--<ItemsControl ItemsSource="{Binding Source={x:Static designData:DesignDataMonitor.Instance}, Path=Monitors[0].Group1}" Style="{StaticResource ItemControlMonitorContentStyle}" />-->
        <ItemsControl Grid.Column="0"
                      ItemsSource="{Binding Path=Domain.MonitorViewModel.Model.OilEngineStartPage.Value.Group1}"
                      Style="{StaticResource ItemControlMonitorContentStyle}" />

        <ItemsControl Grid.Column="1"
                      ItemsSource="{Binding Path=Domain.MonitorViewModel.Model.OilEngineStartPage.Value.Group2}"
                      Style="{StaticResource ItemControlMonitorContentStyle}" />

        <ItemsControl Grid.Column="2"
                      ItemsSource="{Binding Path=Domain.MonitorViewModel.Model.OilEngineStartPage.Value.Group3}"
                      Style="{StaticResource ItemControlMonitorContentStyle}" />

    </Grid>
</UserControl>
