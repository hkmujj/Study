                        <Border BorderBrush="{StaticResource ForegroundWhiteBrush}" BorderThickness="{TemplateBinding BorderThickness}">
                            <ContentPresenter Margin="6,3"
                                              HorizontalAlignment="Center"
                                              Content="{TemplateBinding Content}" />
                        </Border>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style.Setters>
    </Style>

