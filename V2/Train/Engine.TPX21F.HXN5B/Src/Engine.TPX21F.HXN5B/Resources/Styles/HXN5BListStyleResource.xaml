    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="../Base/HXN5BBaseResource.xaml" />
        <ResourceDictionary Source="../Converter/HXN5BConverterResource.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <Style x:Key="ListBoxTestSelfItemStyle" TargetType="ListBox">
        <Style.Setters>
            <Setter Property="FontSize" Value="16" />
            <Setter Property="Background" Value="{x:Null}" />
            <Setter Property="BorderThickness" Value="0" />
            <Setter Property="ScrollViewer.HorizontalScrollBarVisibility" Value="Disabled" />
            <Setter Property="ScrollViewer.VerticalScrollBarVisibility" Value="Disabled" />
            <Setter Property="ItemContainerStyle">
                <Setter.Value>
                    <Style TargetType="ListBoxItem">
                        <Style.Setters>
                            <Setter Property="Template">
                                <Setter.Value>
                                    <ControlTemplate TargetType="ListBoxItem">
                                        <Border Background="{TemplateBinding Background}"
                                                BorderBrush="{x:Null}"
                                                BorderThickness="0"
                                                Padding="3,6"
                                                d:DataContext="{d:DesignInstance detail2:TestSelfItem}">
                                            <TextBlock HorizontalAlignment="Left"
                                                       Style="{StaticResource ContentConstWhiteTextStyle}"
                                                       Text="{Binding Path=ItemConfig.Content}" />
                                        </Border>
                                        <ControlTemplate.Triggers>
                                            <Trigger Property="IsSelected" Value="True">
                                                <Trigger.Setters>
                                                    <Setter Property="Background" Value="{StaticResource SelectedBlueBrush}" />
                                                </Trigger.Setters>
                                            </Trigger>
                                        </ControlTemplate.Triggers>
                                    </ControlTemplate>
                                </Setter.Value>
                            </Setter>
                        </Style.Setters>
                    </Style>
                </Setter.Value>
            </Setter>
        </Style.Setters>
    </Style>

    <Style x:Key="ListBoxTestSelfGroupStyle" TargetType="ListBox">
        <Style.Setters>
            <Setter Property="FontSize" Value="16" />
            <Setter Property="Background" Value="{x:Null}" />
            <Setter Property="BorderThickness" Value="0" />
            <Setter Property="ScrollViewer.HorizontalScrollBarVisibility" Value="Disabled" />
            <Setter Property="ScrollViewer.VerticalScrollBarVisibility" Value="Disabled" />
            <Setter Property="ItemContainerStyle">
                <Setter.Value>
                    <Style TargetType="ListBoxItem">
                        <Style.Setters>
                            <Setter Property="Template">
                                <Setter.Value>
                                    <ControlTemplate TargetType="ListBoxItem">
                                        <Border Background="{TemplateBinding Background}"
                                                BorderBrush="{x:Null}"
                                                BorderThickness="0"
                                                Padding="3,6">
                                            <TextBlock HorizontalAlignment="Left"
                                                       Style="{StaticResource ContentConstWhiteTextStyle}"
                                                       Text="{Binding}" />
                                        </Border>
                                        <ControlTemplate.Triggers>
                                            <Trigger Property="IsSelected" Value="True">
                                                <Trigger.Setters>
                                                    <Setter Property="Background" Value="{StaticResource SelectedBlueBrush}" />
                                                </Trigger.Setters>
                                            </Trigger>
                                        </ControlTemplate.Triggers>
                                    </ControlTemplate>
                                </Setter.Value>
                            </Setter>
                        </Style.Setters>
                    </Style>
                </Setter.Value>
            </Setter>
        </Style.Setters>
    </Style>

    <Style x:Key="ItemControlWheelRSetStyle" TargetType="ItemsControl">
        <Style.Setters>
            <Setter Property="Background" Value="{x:Null}" />
            <Setter Property="FontSize" Value="16" />
            <Setter Property="BorderBrush" Value="{x:Null}" />
            <Setter Property="ScrollViewer.HorizontalScrollBarVisibility" Value="Disabled" />
            <Setter Property="ScrollViewer.VerticalScrollBarVisibility" Value="Disabled" />
            <Setter Property="IsHitTestVisible" Value="False" />
            <Setter Property="Padding" Value="0" />
            <Setter Property="BorderThickness" Value="0" />
            <Setter Property="RenderTransformOrigin" Value="0.5,0.5" />
            <Setter Property="ItemsPanel">
                <Setter.Value>
                    <ItemsPanelTemplate>
                        <StackPanel Orientation="Vertical" />
                    </ItemsPanelTemplate>
                </Setter.Value>
            </Setter>
            <Setter Property="ItemTemplate">
                <Setter.Value>
                    <DataTemplate DataType="detail6:WhellRItem">
                        <Grid Width="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType=ItemsControl}, Path=ActualWidth}">
                            <Grid.Resources>
                                <Style BasedOn="{StaticResource ContentConstGreenTextStyle}" TargetType="TextBlock">
                                    <Style.Setters>
                                        <Setter Property="Margin" Value="3,6" />
                                    </Style.Setters>
                                </Style>
                            </Grid.Resources>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition />
                                <ColumnDefinition />
                                <ColumnDefinition />
                                <ColumnDefinition />
                                <ColumnDefinition />
                                <ColumnDefinition />
                                <ColumnDefinition />
                            </Grid.ColumnDefinitions>
                            <TextBlock Text="{Binding Path=Name}" />
                            <TextBlock Grid.Column="1" Text="{Binding Path=Default, StringFormat={}0}" />
                            <TextBlock Grid.Column="2" Text="{Binding Path=Min, StringFormat={}0}" />
                            <TextBlock Grid.Column="3" Text="{Binding Path=Max, StringFormat={}0}" />
                            <TextBlock Grid.Column="4" Text="{Binding Path=Current, StringFormat={}0}" />
                            <TextBox Grid.Column="5"
                                     Margin="24,3"
                                     Style="{StaticResource InputtingTextStyle}"
                                     Visibility="{Binding Path=IsSetting,
                                                          Converter={StaticResource BooleanToVisibilityConverter}}" />
                            <TextBlock Grid.Column="6" Text="mm" />
                        </Grid>
                    </DataTemplate>
                </Setter.Value>
            </Setter>
        </Style.Setters>
    </Style>

    <Style x:Key="ItemControlAccumulativeDataStyle" TargetType="ItemsControl">
        <Style.Setters>
            <Setter Property="FontSize" Value="16" />
            <Setter Property="Grid.IsSharedSizeScope" Value="True" />
            <Setter Property="ItemsPanel">
                <Setter.Value>
                    <ItemsPanelTemplate>
                        <StackPanel Orientation="Vertical" />
                    </ItemsPanelTemplate>
                </Setter.Value>
            </Setter>
            <Setter Property="ItemTemplate">
                <Setter.Value>
                    <DataTemplate DataType="detail5:AccumulativeDataItem">
                        <Border BorderBrush="{StaticResource ForegroundWhiteBrush}" BorderThickness="1">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" SharedSizeGroup="AccumulativeDataItemContentName" />
                                    <ColumnDefinition Width="1*" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>
                                <TextBlock Margin="2,4"
                                           FontSize="{Binding RelativeSource={RelativeSource FindAncestor,
                                                                                             AncestorType=ItemsControl},
                                                              Path=FontSize}"
                                           Style="{StaticResource ContentConstGreenTextStyle}"
                                           Text="{Binding Path=ItemConfig.ShowContent}"
                                           TextAlignment="Left" />
                                <TextBlock Grid.Column="1"
                                           FontSize="{Binding RelativeSource={RelativeSource FindAncestor,
                                                                                             AncestorType=ItemsControl},
                                                              Path=FontSize}"
                                           Style="{StaticResource ContentConstWhiteTextStyle}">
                                    <TextBlock.Text>
                                        <MultiBinding Converter="{StaticResource AccumulativeValueToStringConverter}">
                                            <Binding Path="(common:AccumulativeDataUnit.Unit)" RelativeSource="{RelativeSource FindAncestor, AncestorType=ItemsControl}" />
                                            <Binding Path="Value" />
                                        </MultiBinding>
                                    </TextBlock.Text>
                                </TextBlock>
                                <TextBlock Grid.Column="2"
                                           Margin="2"
                                           FontSize="{Binding RelativeSource={RelativeSource FindAncestor,
                                                                                             AncestorType=ItemsControl},
                                                              Path=FontSize}"
                                           Style="{StaticResource ContentConstGreenTextStyle}"
                                           Text="{Binding RelativeSource={RelativeSource FindAncestor,
                                                                                         AncestorType=ItemsControl},
                                                          Path=(common:AccumulativeDataUnit.Unit)}" />
                            </Grid>
                        </Border>
                    </DataTemplate>
                </Setter.Value>
            </Setter>
        </Style.Setters>
    </Style>


    <Style x:Key="ItemControlPortDataStyle" TargetType="ItemsControl">
