                                </Style>
                                <Style TargetType="Border">
                                    <Setter Property="Margin" Value="2,2,1,1" />
                                    <Setter Property="BorderBrush" Value="{StaticResource ForegroundWhiteBrush}" />
                                    <Setter Property="BorderThickness" Value="0,0,2,2" />
                                    <Setter Property="Panel.ZIndex" Value="1" />
                                </Style>
                            </Grid.Resources>

                            <Border />
                            <Label Grid.Column="0" Content="{Binding Path=Port}" />
                            <Border Grid.Column="1" Grid.ColumnSpan="4" />
                            <Label Grid.Column="1 ">
                                <Label.Content>
                                    <MultiBinding Converter="{StaticResource PortDataStyleToStringFormatConverter}">
                                        <Binding Path="D0" />
                                        <Binding Path="DataStyle" Source="{x:Static designData:DesignPortData.Instance}" />
                                    </MultiBinding>
