                                       Width="{Binding Lenght}"
                                       Height="2"
                                       HorizontalAlignment="Left"
                                       VerticalAlignment="Center"
                                       Fill="White" />

                            <Grid Grid.Column="1">
                                <StackPanel HorizontalAlignment="Left" Orientation="Horizontal">
                                    <TextBlock Width="5"
                                               Opacity="1"
                                               Visibility="Hidden" />
                                    <TextBlock Width="40"
                                               Height="25"
                                               HorizontalAlignment="Left"
                                               RenderTransformOrigin="0.5,0.5"
                                               Style="{StaticResource ContentBrakeSysGreenSmallTextStyle}"
                                               Text="{Binding Text}">
                                        <TextBlock.RenderTransform>
                                            <RotateTransform Angle="{Binding Angle, Converter={StaticResource DegreeScaleTextAngleConverter}}" />
                                        </TextBlock.RenderTransform>
                                    </TextBlock>
                                </StackPanel>
                            </Grid>
                        </Grid>
                    </DataTemplate>
                </Setter.Value>
            </Setter>
        </Style.Setters>
    </Style>

    <Style x:Key="ItemControlDialPlatInSpeedTestStyle" TargetType="ItemsControl">
        <Style.Setters>
            <Setter Property="RenderTransformOrigin" Value="0.5,0.5" />
