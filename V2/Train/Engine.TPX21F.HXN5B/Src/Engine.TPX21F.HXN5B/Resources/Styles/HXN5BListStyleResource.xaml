                                        <Binding Path="D6 " />
                                        <Binding Path="DataStyle" Source="{x:Static designData:DesignPortData.Instance}" />
                                    </MultiBinding>
                                </Label.Content>
                            </Label>
                            <Label Grid.Column="8 ">
                                <Label.Content>
