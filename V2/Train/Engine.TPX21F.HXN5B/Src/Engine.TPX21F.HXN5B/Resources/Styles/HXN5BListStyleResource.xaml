                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="1.1*" />
                                        <ColumnDefinition />
                                        <ColumnDefinition />
                                        <ColumnDefinition />
                                        <ColumnDefinition />
                                        <ColumnDefinition />
                                        <ColumnDefinition />
                                        <ColumnDefinition />
                                        <ColumnDefinition />
                                        <ColumnDefinition />
                                        <ColumnDefinition />
                                        <ColumnDefinition />
                                        <ColumnDefinition />
                                        <ColumnDefinition />
                                        <ColumnDefinition />
                                        <ColumnDefinition />
                                        <ColumnDefinition />
                                    </Grid.ColumnDefinitions>
                                    <Grid.Resources>
                                        <Style TargetType="Label">
                                            <Setter Property="Margin" Value="0" />
                                            <Setter Property="Padding" Value="3" />
                                            <Setter Property="BorderThickness" Value="1,1,0,1" />
                                            <Setter Property="BorderBrush" Value="WhiteSmoke" />
                                            <Setter Property="Background" Value="Black" />
                                            <Setter Property="Foreground" Value="{StaticResource ForegroundWhiteBrush}" />
                                            <Setter Property="Template">
                                                <Setter.Value>
                                                    <ControlTemplate TargetType="Label">
                                                        <Border Background="{TemplateBinding Background}"
                                                                BorderBrush="{TemplateBinding BorderBrush}"
                                                                BorderThickness="{TemplateBinding BorderThickness}">
                                                            <ContentPresenter Margin="{TemplateBinding Padding}" />
                                                        </Border>
                                                    </ControlTemplate>
                                                </Setter.Value>
                                            </Setter>
                                        </Style>
                                    </Grid.Resources>
                                    <Label BorderThickness="1" Content="Port" />
                                    <Label Grid.Column="1 "
                                           Margin="2,0,0,0"
                                           Content="D0" />
                                    <Label Grid.Column="2 " Content="D1" />
                                    <Label Grid.Column="3 " Content="D2" />
                                    <Label Grid.Column="4 "
                                           BorderThickness="1"
                                           Content="D3" />
                                    <Label Grid.Column="5 "
                                           Margin="2,0,0,0"
                                           Content="D4" />
                                    <Label Grid.Column="6 " Content="D5" />
                                    <Label Grid.Column="7 " Content="D6" />
                                    <Label Grid.Column="8 "
                                           BorderThickness="1"
                                           Content="D7" />
                                    <Label Grid.Column="9 "
                                           Margin="2,0,0,0"
                                           Content="D8" />
                                    <Label Grid.Column="10" Content="D9" />
                                    <Label Grid.Column="11" Content="D10" />
                                    <Label Grid.Column="12"
                                           BorderThickness="1"
                                           Content="D11" />
                                    <Label Grid.Column="13"
                                           Margin="2,0,0,0"
                                           Content="D12" />
                                    <Label Grid.Column="14" Content="D13" />
                                    <Label Grid.Column="15" Content="D14" />
                                    <Label Grid.Column="16"
                                           BorderThickness="1"
                                           Content="D15" />
                                </Grid>
                                <ItemsPresenter Grid.Row="1"
                                                Margin="0"
                                                IsHitTestVisible="{TemplateBinding IsHitTestVisible}" />
                            </Grid>
                        </Border>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
            <Setter Property="ItemsPanel">
                <Setter.Value>
                    <ItemsPanelTemplate>
                        <StackPanel Orientation="Vertical" />
                    </ItemsPanelTemplate>
                </Setter.Value>
            </Setter>

            <Setter Property="ItemTemplate">
                <Setter.Value>
                    <DataTemplate DataType="detail6:PortDataItem">
                        <Grid Width="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType=ItemsControl}, Path=ActualWidth}">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="1.1*" />
                                <ColumnDefinition />
                                <ColumnDefinition />
                                <ColumnDefinition />
                                <ColumnDefinition />
                                <ColumnDefinition />
                                <ColumnDefinition />
                                <ColumnDefinition />
                                <ColumnDefinition />
                                <ColumnDefinition />
                                <ColumnDefinition />
                                <ColumnDefinition />
                                <ColumnDefinition />
                                <ColumnDefinition />
                                <ColumnDefinition />
                                <ColumnDefinition />
                                <ColumnDefinition />
                            </Grid.ColumnDefinitions>
                            <Grid.Resources>
                                <Style TargetType="Label">
                                    <Setter Property="Margin" Value="1,1,0,0" />
                                    <Setter Property="Padding" Value="3" />
                                    <Setter Property="BorderThickness" Value="1,1,0,0" />
                                    <Setter Property="BorderBrush" Value="WhiteSmoke" />
                                    <Setter Property="Background" Value="Black" />
                                    <Setter Property="Foreground" Value="{StaticResource ForegroundWhiteBrush}" />
