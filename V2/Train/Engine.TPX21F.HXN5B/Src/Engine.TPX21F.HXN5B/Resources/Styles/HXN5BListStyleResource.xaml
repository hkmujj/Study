                                </Label.Content>
                            </Label>
                            <Border Grid.Column="13" Grid.ColumnSpan="4" />
                            <Label Grid.Column="13">
                                <Label.Content>
                                    <MultiBinding Converter="{StaticResource PortDataStyleToStringFormatConverter}">
                                        <Binding Path="D12" />
                                        <Binding Path="DataStyle" Source="{x:Static designData:DesignPortData.Instance}" />
                                    </MultiBinding>
                                </Label.Content>
                            </Label>
                            <Label Grid.Column="14">
                                <Label.Content>
                                    <MultiBinding Converter="{StaticResource PortDataStyleToStringFormatConverter}">
