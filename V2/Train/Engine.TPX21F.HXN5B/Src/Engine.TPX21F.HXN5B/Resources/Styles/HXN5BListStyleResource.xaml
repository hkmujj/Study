                                        <Binding Path="D2 " />
                                        <Binding Path="DataStyle" Source="{x:Static designData:DesignPortData.Instance}" />
                                    </MultiBinding>
                                </Label.Content>
                            </Label>
                            <Label Grid.Column="4 ">
