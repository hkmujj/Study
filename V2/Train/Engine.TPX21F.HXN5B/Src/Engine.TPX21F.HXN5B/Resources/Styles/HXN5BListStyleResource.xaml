                                <Label.Content>
                                    <MultiBinding Converter="{StaticResource PortDataStyleToStringFormatConverter}">
                                        <Binding Path="D8 " />
                                        <Binding Path="DataStyle" Source="{x:Static designData:DesignPortData.Instance}" />
                                    </MultiBinding>
                                </Label.Content>
                            </Label>
                            <Label Grid.Column="10">
                                <Label.Content>
                                    <MultiBinding Converter="{StaticResource PortDataStyleToStringFormatConverter}">
                                        <Binding Path="D9 " />
                                        <Binding Path="DataStyle" Source="{x:Static designData:DesignPortData.Instance}" />
                                    </MultiBinding>
                                </Label.Content>
                            </Label>
                            <Label Grid.Column="11">
                                <Label.Content>
                                    <MultiBinding Converter="{StaticResource PortDataStyleToStringFormatConverter}">
                                        <Binding Path="D10" />
                                        <Binding Path="DataStyle" Source="{x:Static designData:DesignPortData.Instance}" />
                                    </MultiBinding>
                                </Label.Content>
                            </Label>
                            <Label Grid.Column="12">
