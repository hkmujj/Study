<UserControl x:Class="Urban.Phillippine.View.ComViews.TrainAngle"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Urban.Phillippine.View.ComViews"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             d:DesignHeight="30"
             d:DesignWidth="30"
             mc:Ignorable="d">
    <UserControl.Resources>
        <ResourceDictionary Source="../Resource/PhilippineResource.xaml" />
    </UserControl.Resources>
    <Viewbox Width="{Binding ElementName=Control, Path=ActualWidth}"
             Height="{Binding ElementName=Control, Path=ActualHeight}">
        <Border BorderBrush="{StaticResource LogicGrayBrush}">
            <Canvas Width="30"
                    Height="30"
                    Margin="10">
                <Path Data="M 1,16 L 29,1 L 29,29" Fill="Black" />
            </Canvas>
        </Border>
    </Viewbox>
</UserControl>