<UserControl x:Class="Urban.Phillippine.View.Views.MainContentShell"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Urban.Phillippine.View.Views"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:name="clr-namespace:Urban.Phillippine.View.Constant"
             xmlns:region="http://www.codeplex.com/prism"
             d:DesignHeight="600"
             d:DesignWidth="800"
             mc:Ignorable="d">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="80*" />
            <RowDefinition Height="500*" />
        </Grid.RowDefinitions>
        <ContentControl region:RegionManager.RegionName="{x:Static name:RegionNames.TitleRegion}" />
        <ContentControl Grid.Row="1" region:RegionManager.RegionName="{x:Static name:RegionNames.ContentAndButtonRegion}" />
    </Grid>
</UserControl>