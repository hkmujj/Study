<UserControl x:Class="LightRail.HMI.GZYGDC.View.Layout.ShellContentStyle1Layout"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:constant="clr-namespace:LightRail.HMI.GZYGDC.Constant"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:regions="http://www.codeplex.com/CompositeWPF"
             d:DesignHeight="600"
             d:DesignWidth="800"
             mc:Ignorable="d">
    <Grid>
        <Border Background="{StaticResource ShellBackgroundBrush}" 
                regions:RegionManager.RegionName="{x:Static constant:RegionNames.ShellContent}" />
    </Grid>
</UserControl>
