<UserControl
    x:Class="Subway.CBTC.Casco.View.Contents.RegionA.RegionA1"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:viewModel="clr-namespace:Subway.CBTC.Casco.ViewModel"
    d:DataContext="{d:DesignInstance viewModel:DomainViewModel}"
    d:DesignHeight="50"
    d:DesignWidth="50"
    mc:Ignorable="d">
    <Grid>
        <Border Background="{Binding CBTC.SignalInfo.Speed.BrakeDetailsType, Converter={StaticResource Subway.CBTC.Casco.Converter.BrakeDetailsTypeToBrush}}" />
    </Grid>
</UserControl>
