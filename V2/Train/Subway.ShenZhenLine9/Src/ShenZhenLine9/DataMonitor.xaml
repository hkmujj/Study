<Window x:Class="Subway.ShenZhenLine9.DataMonitor"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:dxprg="http://schemas.devexpress.com/winfx/2008/xaml/propertygrid"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:viewModels="clr-namespace:Subway.ShenZhenLine9.ViewModels"
        Title="DataMonitor"
        Width="300"
        Height="300"
        d:DataContext="{d:DesignInstance viewModels:ShenZhenLine9ViewModel}"
        mc:Ignorable="d">
    <Grid>
        <dxprg:PropertyGridControl AllowExpanding="ForceIfNoTypeConverter"
                                   AllowInstanceInitializer="False"
                                   SelectedObject="{Binding}"
                                   ShowCategories="False" />
    </Grid>
</Window>
