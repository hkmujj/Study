<UserControl x:Class="Subway.ShenZhenLine9.Views.MainContent.HelpView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:constance="clr-namespace:Subway.ShenZhenLine9.Constance"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Subway.ShenZhenLine9.Views.MainContent"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:regions="http://www.codeplex.com/CompositeWPF"
             d:DesignHeight="480"
             d:DesignWidth="800"
             mc:Ignorable="d">
    <Grid>
        <TabControl Margin="15"
                    regions:RegionManager.RegionName="{x:Static constance:RegionNames.HelpRegion}"
                    Style="{StaticResource Subway.ShenZhenLine9.TabControlStyle}" />
    </Grid>
</UserControl>
