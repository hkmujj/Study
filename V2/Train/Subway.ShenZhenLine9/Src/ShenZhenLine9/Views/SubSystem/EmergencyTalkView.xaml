<UserControl
    x:Class="Subway.ShenZhenLine9.Views.SubSystem.EmergencyTalkView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:car="clr-namespace:Subway.ShenZhenLine9.Views.Car"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:viewModels="clr-namespace:Subway.ShenZhenLine9.ViewModels"
    d:DataContext="{d:DesignInstance viewModels:ShenZhenLine9ViewModel}"
    d:DesignHeight="115"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="52*" />
            <ColumnDefinition Width="18*" />
            <ColumnDefinition Width="110*" />
            <ColumnDefinition Width="110*" />
            <ColumnDefinition Width="110*" />
            <ColumnDefinition Width="110*" />
            <ColumnDefinition Width="110*" />
            <ColumnDefinition Width="110*" />
            <ColumnDefinition Width="18*" />
            <ColumnDefinition Width="52*" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="32*" />
            <RowDefinition Height="16*" />
            <RowDefinition Height="21*" />
            <RowDefinition Height="10*" />
            <RowDefinition Height="21*" />
            <RowDefinition Height="16*" />
        </Grid.RowDefinitions>
        <car:CommonCar
            Grid.Row="0"
            Grid.RowSpan="6"
            Grid.Column="0"
            Grid.ColumnSpan="10"
            Panel.ZIndex="-1" />
        <ItemsControl
            Grid.Row="1"
            Grid.RowSpan="2"
            Grid.Column="2"
            ItemsSource="{Binding MasterPageViewModel.EmergencyTalkViewModel.Car1Top}"
            Style="{StaticResource Subway.ShenZhenLine9.EmegcnyTalkTopStyle}" />

        <ItemsControl
            Grid.Row="1"
            Grid.RowSpan="2"
            Grid.Column="3"
            ItemsSource="{Binding MasterPageViewModel.EmergencyTalkViewModel.Car2Top}"
            Style="{StaticResource Subway.ShenZhenLine9.EmegcnyTalkTopStyle}" />

        <ItemsControl
            Grid.Row="1"
            Grid.RowSpan="2"
            Grid.Column="4"
            ItemsSource="{Binding MasterPageViewModel.EmergencyTalkViewModel.Car3Top}"
            Style="{StaticResource Subway.ShenZhenLine9.EmegcnyTalkTopStyle}" />

        <ItemsControl
            Grid.Row="1"
            Grid.RowSpan="2"
            Grid.Column="5"
            ItemsSource="{Binding MasterPageViewModel.EmergencyTalkViewModel.Car4Top}"
            Style="{StaticResource Subway.ShenZhenLine9.EmegcnyTalkTopStyle}" />

        <ItemsControl
            Grid.Row="1"
            Grid.RowSpan="2"
            Grid.Column="6"
            ItemsSource="{Binding MasterPageViewModel.EmergencyTalkViewModel.Car5Top}"
            Style="{StaticResource Subway.ShenZhenLine9.EmegcnyTalkTopStyle}" />

        <ItemsControl
            Grid.Row="1"
            Grid.RowSpan="2"
            Grid.Column="7"
            ItemsSource="{Binding MasterPageViewModel.EmergencyTalkViewModel.Car6Top}"
            Style="{StaticResource Subway.ShenZhenLine9.EmegcnyTalkTopStyle}" />
        <ItemsControl
            Grid.Row="4"
            Grid.RowSpan="2"
            Grid.Column="2"
            ItemsSource="{Binding MasterPageViewModel.EmergencyTalkViewModel.Car1Buttom}"
            Style="{StaticResource Subway.ShenZhenLine9.EmegcnyTalkButtomStyle}" />
        <ItemsControl
            Grid.Row="4"
            Grid.RowSpan="2"
            Grid.Column="3"
            ItemsSource="{Binding MasterPageViewModel.EmergencyTalkViewModel.Car2Buttom}"
            Style="{StaticResource Subway.ShenZhenLine9.EmegcnyTalkButtomStyle}" />
        <ItemsControl
            Grid.Row="4"
            Grid.RowSpan="2"
            Grid.Column="4"
            ItemsSource="{Binding MasterPageViewModel.EmergencyTalkViewModel.Car3Buttom}"
            Style="{StaticResource Subway.ShenZhenLine9.EmegcnyTalkButtomStyle}" />
        <ItemsControl
            Grid.Row="4"
            Grid.RowSpan="2"
            Grid.Column="5"
            ItemsSource="{Binding MasterPageViewModel.EmergencyTalkViewModel.Car4Buttom}"
            Style="{StaticResource Subway.ShenZhenLine9.EmegcnyTalkButtomStyle}" />
        <ItemsControl
            Grid.Row="4"
            Grid.RowSpan="2"
            Grid.Column="6"
            ItemsSource="{Binding MasterPageViewModel.EmergencyTalkViewModel.Car5Buttom}"
            Style="{StaticResource Subway.ShenZhenLine9.EmegcnyTalkButtomStyle}" />
        <ItemsControl
            Grid.Row="4"
            Grid.RowSpan="2"
            Grid.Column="7"
            ItemsSource="{Binding MasterPageViewModel.EmergencyTalkViewModel.Car6Buttom}"
            Style="{StaticResource Subway.ShenZhenLine9.EmegcnyTalkButtomStyle}" />
    </Grid>
</UserControl>
