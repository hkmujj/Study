<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="../Base/CRH380BGBaseResource.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <Style x:Key="Motor.HMI.CRH380BG.LeftDownBorderStyle" TargetType="Border">
        <Style.Setters>
            <Setter Property="BorderBrush" Value="{StaticResource Motor.HMI.CRH380BG.ForegroundWhiteBrush}" />
            <Setter Property="BorderThickness" Value="1,0,0,1" />
        </Style.Setters>
    </Style>

    <Style x:Key="Motor.HMI.CRH380BG.LeftBorderStyle" TargetType="Border">
        <Style.Setters>
            <Setter Property="BorderBrush" Value="{StaticResource Motor.HMI.CRH380BG.ForegroundWhiteBrush}" />
            <Setter Property="BorderThickness" Value="1,0,0,0" />
        </Style.Setters>
    </Style>

    <Style x:Key="Motor.HMI.CRH380BG.BrakeEfficientBorderDarkOrange" TargetType="Border">
        <Style.Setters>
            <Setter Property="BorderThickness" Value="2" />
            <Setter Property="Background" Value="DarkOrange" />
            <Setter Property="BorderBrush" Value="White" />
            <Setter Property="Margin" Value="5,2,5,0" />
        </Style.Setters>
    </Style>

    
    
    <Style x:Key="Motor.HMI.CRH380BG.BrakeEfficientBorderBlue" TargetType="Border">
        <Style.Setters>
            <Setter Property="BorderThickness" Value="2" />
            <Setter Property="Background" Value="Blue" />
            <Setter Property="BorderBrush" Value="White" />
            <Setter Property="Margin" Value="5,2,5,0" />
        </Style.Setters>
    </Style>

    <Style x:Key="Motor.HMI.CRH380BG.AirConditionBorderStyle" TargetType="Border">
        <Style.Setters>
            <Setter Property="BorderBrush" Value="{StaticResource Motor.HMI.CRH380BG.ContentBackgroundTransparentBrush}"/>
        </Style.Setters>
    </Style>
    
    <Style x:Key="Motor.HMI.CRH380BG.BorderStyle" TargetType="Border">
        <Style.Setters>
            <Setter Property="BorderBrush" Value="{StaticResource Motor.HMI.CRH380BG.ForegroundWhiteBrush}" />
            <Setter Property="BorderThickness" Value="1" />
        </Style.Setters>
    </Style>

    <Style x:Key="Motor.HMI.CRH380BG.WhiteBorderStyle" TargetType="Border">
        <Style.Setters>
            <Setter Property="Background" Value="{StaticResource Motor.HMI.CRH380BG.ForegroundWhiteBrush}" />
        </Style.Setters>
    </Style>

    <Style x:Key="Motor.HMI.CRH380BG.VerticalRectangle" TargetType="Rectangle">
        <Style.Setters>
            <Setter Property="Width" Value="1" />
            <Setter Property="Fill" Value="White" />

            <Setter Property="HorizontalAlignment" Value="Left" />
            <Setter Property="VerticalAlignment" Value="Stretch" />
        </Style.Setters>
    </Style>

    <Style x:Key="Motor.HMI.CRH380BG.HorizontalRectangle" TargetType="Rectangle">
        <Style.Setters>
            <Setter Property="Height" Value="1" />
            <Setter Property="Fill" Value="White" />
            <Setter Property="HorizontalAlignment" Value="Stretch" />
            <Setter Property="VerticalAlignment" Value="Bottom" />
        </Style.Setters>
    </Style>

    <Style x:Key="LineStyle" TargetType="Line">
        <Setter Property="Fill" Value="{StaticResource Motor.HMI.CRH380BG.ForegroundWhiteBrush}" />
        <Setter Property="Stroke" Value="{StaticResource Motor.HMI.CRH380BG.ForegroundWhiteBrush}" />
        <Setter Property="StrokeThickness" Value="1" />
    </Style>
</ResourceDictionary>