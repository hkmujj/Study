<UserControl x:Class="Motor.HMI.CRH380BG.View.Contents.Info.MaintainInfoView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:viewModel="clr-namespace:Motor.HMI.CRH380BG.ViewModel"
             Background="{StaticResource Motor.HMI.CRH380BG.ContentBackgroundBrush}"
             d:DataContext="{d:DesignInstance viewModel:CRH380BGViewModel}"
             d:DesignHeight="495"
             d:DesignWidth="800"
             mc:Ignorable="d" >
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="40*"/>
            <RowDefinition Height="40*"/>
            <RowDefinition Height="415*"/>
        </Grid.RowDefinitions>
        <TextBlock Margin="0,0,30,0" Style="{StaticResource Motor.HMI.CRH380BG.TitleTextStyle}" HorizontalAlignment="Right" Text="第 1 行 共 1 行"></TextBlock>
        <Image Grid.Row="1" Height="30" HorizontalAlignment="Left" Source="{StaticResource Motor.HMI.CRH380BG.ImgRedTriangleRight}"></Image>
        <TextBlock Grid.Row="1" Margin="40,0,0,0" HorizontalAlignment="Left" Style="{StaticResource Motor.HMI.CRH380BG.TitleTextStyle}" Text="无有用信息"></TextBlock>
        <Image Grid.Row="1" Height="30" Source="{StaticResource Motor.HMI.CRH380BG.ImgRedTriangleLeft}" HorizontalAlignment="Right"></Image>
    </Grid>
</UserControl>