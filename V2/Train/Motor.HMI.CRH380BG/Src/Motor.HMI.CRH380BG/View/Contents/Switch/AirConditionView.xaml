            <Grid.RowDefinitions>
                <RowDefinition Height="165*" />
                <RowDefinition Height="330*" />
            </Grid.RowDefinitions>
            <Border BorderBrush="{StaticResource Motor.HMI.CRH380BG.ForegroundWhiteBrush}" BorderThickness="1">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                    </Grid.RowDefinitions>
                    <StackPanel Margin="5,0,0,0" Orientation="Horizontal">
                        <Border Width="25"
                                Height="25"
                                BorderBrush="{StaticResource Motor.HMI.CRH380BG.ForegroundWhiteBrush}"
                                BorderThickness="1" />
                        <TextBlock Margin="5,0,0,0"
                                   Style="{StaticResource Motor.HMI.CRH380BG.TitleTextStyle}"
                                   Text="{StaticResource Motor.HMI.CRH380BG.StringAirConditionClose}" />
                    </StackPanel>
                    <StackPanel Grid.Row="1"
                                Margin="5,0,0,0"
                                Orientation="Horizontal">
                        <Border Width="25"
                                Height="25"
                                Background="{StaticResource Motor.HMI.CRH380BG.ContentLimeGreenBrush}"
                                BorderBrush="{StaticResource Motor.HMI.CRH380BG.ForegroundWhiteBrush}"
                                BorderThickness="1" />
                        <TextBlock Margin="5,0,0,0"
                                   Style="{StaticResource Motor.HMI.CRH380BG.TitleTextStyle}"
                                   Text="{StaticResource Motor.HMI.CRH380BG.StringAutoOpenAirCondition}" />
                    </StackPanel>
                    <StackPanel Grid.Row="2"
                                Margin="5,0,0,0"
                                Orientation="Horizontal">
                        <Border Width="25"
                                Height="25"
                                Background="{StaticResource Motor.HMI.CRH380BG.ContentBackgroundYellowBrush}"
                                BorderBrush="{StaticResource Motor.HMI.CRH380BG.ForegroundWhiteBrush}"
                                BorderThickness="1" />
                        <TextBlock Margin="5,0,0,0"
                                   Style="{StaticResource Motor.HMI.CRH380BG.TitleTextStyle}"
                                   Text="{StaticResource Motor.HMI.CRH380BG.StringManiualOpenAirCondition}" />
                    </StackPanel>
                    <StackPanel Grid.Row="3"
                                Margin="5,0,0,0"
                                Orientation="Horizontal">
                        <Border Width="25"
                                Height="25"
                                BorderBrush="{StaticResource Motor.HMI.CRH380BG.ForegroundWhiteBrush}"
                                BorderThickness="1">
                            <Image Source="{StaticResource Motor.HMI.CRH380BG.ImgAirEmergency}" />
                        </Border>
                        <TextBlock Margin="5,0,0,0"
                                   Style="{StaticResource Motor.HMI.CRH380BG.TitleTextStyle}"
                                   Text="{StaticResource Motor.HMI.CRH380BG.StringTemperatureSetValue}" />
                    </StackPanel>
                    <StackPanel Grid.Row="4"
                                Margin="5,0,0,0"
                                Orientation="Horizontal">
                        <Border Width="25"
                                Height="25"
                                Background="{StaticResource Motor.HMI.CRH380BG.ContentBackgroundRedBrush}"
                                BorderBrush="{StaticResource Motor.HMI.CRH380BG.ForegroundWhiteBrush}"
                                BorderThickness="1" />
                        <TextBlock Margin="5,0,0,0"
                                   Style="{StaticResource Motor.HMI.CRH380BG.TitleTextStyle}"
                                   Text="{StaticResource Motor.HMI.CRH380BG.StringAirConditionEmergency}" />
                    </StackPanel>
                </Grid>
            </Border>
        </Grid>
    </Grid>
</UserControl>
