                                                           Converter={StaticResource Motor.HMI.CRH380BG.TraBraPercentBorderTypeConverter}}"
                                     Foreground="{Binding Path=Domain.Model.MainData.TractionBrakeType,
                                                          Converter={StaticResource Motor.HMI.CRH380BG.TraBraPercentForTypeConverter}}"
                                     Style="{StaticResource Motor.HMI.CRH380BG.MainViewTractionProgressBarStyle}"
                                     Value="{Binding Path=Domain.Model.MainData.TrctionBrakeActualPercent1}" />
                        <Canvas Grid.Column="3" Panel.ZIndex="1">
                            <Image Canvas.Right="1"
                                   Width="20"
                                   Height="{Binding RelativeSource={RelativeSource FindAncestor,
                                                                                   AncestorType=Canvas},
                                                    Path=ActualWidth}"
                                   Source="{Binding Path=Domain.Model.MainData.TractionBrakeType,
                                                    Converter={StaticResource Motor.HMI.CRH380BG.TraBraPercentLeftImgTypeConverter}}">
                                <Canvas.Top>
                                    <MultiBinding Converter="{StaticResource Motor.HMI.CRH380BG.SetValueConverter}">
                                        <Binding Path="Domain.Model.MainData.TrctionBrakeSetPercent1" />
                                        <Binding Path="ActualHeight" RelativeSource="{RelativeSource FindAncestor, AncestorType=Canvas}" />
                                    </MultiBinding>
                                </Canvas.Top>
                            </Image>
                        </Canvas>
                    </Grid>
                </Grid>
            </Border>
        </Grid>
        <TextBlock Grid.Column="6"
                   Margin="10,0,0,55"
                   HorizontalAlignment="Left"
                   VerticalAlignment="Bottom"
                   Style="{StaticResource Motor.HMI.CRH380BG.TrainConfigEnTextSyle}"
                   Text="%" />
        <Grid Grid.Column="7">
            <Grid.RowDefinitions>
                <RowDefinition Height="165*" />
                <RowDefinition Height="330*" />
            </Grid.RowDefinitions>
            <Border Margin="0,10,10,0"
                    BorderBrush="{StaticResource Motor.HMI.CRH380BG.ForegroundWhiteBrush}"
                    BorderThickness="1">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                    </Grid.RowDefinitions>
                    <TextBlock HorizontalAlignment="Left"
                               Style="{StaticResource Motor.HMI.CRH380BG.TrainConfigEnTextSyle}"
                               Text="{StaticResource Motor.HMI.CRH380BG.StringTractionPower}" />
                    <StackPanel Grid.Row="1" Orientation="Horizontal">
                        <TextBlock Margin="5,0,0,0"
                                   Style="{StaticResource Motor.HMI.CRH380BG.TrainConfigEnTextSyle}"
                                   Text="{StaticResource Motor.HMI.CRH380BG.StringActualValue}" />
                        <Border Width="20"
                                Height="20"
                                Margin="5,0,0,0"
                                Background="{StaticResource Motor.HMI.CRH380BG.BackgroundBlueBrsh}"
                                BorderBrush="{StaticResource Motor.HMI.CRH380BG.ForegroundWhiteBrush}"
                                BorderThickness="1" />
                    </StackPanel>
                    <StackPanel Grid.Row="2" Orientation="Horizontal">
                        <TextBlock Margin="5,0,0,0"
                                   Style="{StaticResource Motor.HMI.CRH380BG.TrainConfigEnTextSyle}"
                                   Text="{StaticResource Motor.HMI.CRH380BG.StringSetValue}" />
                        <Border Width="20"
                                Height="20"
                                Margin="5,0,0,0">
                            <Image Source="{StaticResource Motor.HMI.CRH380BG.ImgRedTriangleLeft}" />
                        </Border>
                    </StackPanel>
                    <TextBlock Grid.Row="3"
                               HorizontalAlignment="Left"
                               Style="{StaticResource Motor.HMI.CRH380BG.TrainConfigEnTextSyle}"
                               Text="{StaticResource Motor.HMI.CRH380BG.StringElectricPower}" />
                    <StackPanel Grid.Row="4" Orientation="Horizontal">
                        <TextBlock Margin="5,0,0,0"
                                   Style="{StaticResource Motor.HMI.CRH380BG.TrainConfigEnTextSyle}"
                                   Text="{StaticResource Motor.HMI.CRH380BG.StringActualValue}" />
                        <Border Width="20"
                                Height="20"
                                Margin="5,0,0,0"
                                Background="{StaticResource Motor.HMI.CRH380BG.ContentBackgroundOrangeBrush}"
                                BorderBrush="{StaticResource Motor.HMI.CRH380BG.ForegroundWhiteBrush}"
                                BorderThickness="1" />
                    </StackPanel>
                    <StackPanel Grid.Row="5" Orientation="Horizontal">
                        <TextBlock Margin="5,0,0,0"
                                   Style="{StaticResource Motor.HMI.CRH380BG.TrainConfigEnTextSyle}"
                                   Text="{StaticResource Motor.HMI.CRH380BG.StringSetValue}" />
                        <Border Width="20"
                                Height="20"
                                Margin="5,0,0,0">
                            <Image Source="{StaticResource Motor.HMI.CRH380BG.ImgGreenTriangleLeft}" />
                        </Border>
                    </StackPanel>
                </Grid>
            </Border>
        </Grid>
    </Grid>
</UserControl>
