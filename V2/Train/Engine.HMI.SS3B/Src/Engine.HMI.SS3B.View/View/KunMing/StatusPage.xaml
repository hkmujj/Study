            <RowDefinition Height="1*" />
            <RowDefinition Height="1*" />
            <RowDefinition Height="1*" />
            <RowDefinition Height="1*" />
        </Grid.RowDefinitions>
        <Rectangle Grid.Row="0"
                   Grid.RowSpan="18"
                   Margin="-1"
                   Stroke="{StaticResource BlueBruh}"
                   StrokeThickness="2" />
