<UserControl x:Class="Engine.TAX2.SS7C.View.Contents.ModifyTimeView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:view="clr-namespace:MMI.Facility.WPFInfrastructure.View;assembly=MMI.Facility.WPFInfrastructure"
             xmlns:viewModel="clr-namespace:Engine.TAX2.SS7C.ViewModel"
             d:DataContext="{d:DesignInstance viewModel:SS7CViewModel}"
             d:DesignHeight="40"
             d:DesignWidth="270"
             Background="{StaticResource ForegroundWhiteBrush}"
             BorderThickness="1"
             Style="{StaticResource ContentControlBulgeFrameStyle}"
             mc:Ignorable="d">
    <view:CaretTextBox Height="40"
                       AlwaysShowCaret="True"
                       CaretLocation="Bottom"
                       CaretTextModel="{Binding Path=OtherViewModel.Model.ModifyTimeModel}"
                       Style="{StaticResource ContentModifyDateTimeTextStyle}" />
</UserControl>