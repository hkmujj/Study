<UserControl x:Class="Engine.TAX2.SS7C.View.Layout.ShellContentStyle1Layout"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:constant="clr-namespace:Engine.TAX2.SS7C.Constant"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:regions="http://www.codeplex.com/CompositeWPF"
             d:DesignHeight="600"
             d:DesignWidth="800"
             mc:Ignorable="d">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="550*" />
            <RowDefinition Height="50*" />
        </Grid.RowDefinitions>
        <Border regions:RegionManager.RegionName="{x:Static constant:RegionNames.ContentUpContent}" />
        <Border Grid.Row="1" regions:RegionManager.RegionName="{x:Static constant:RegionNames.ContentDownButton}" />
    </Grid>
</UserControl>
