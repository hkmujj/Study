<UserControl x:Class="Engine.TAX2.SS7C.View.Layout.ShellContentStyle2Layout"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:constant="clr-namespace:Engine.TAX2.SS7C.Constant"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:regions="http://www.codeplex.com/CompositeWPF"
             d:DesignHeight="600"
             d:DesignWidth="800"
             Background="{StaticResource ContentBackground2Brush}"
             mc:Ignorable="d">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="37*" />
            <RowDefinition Height="113*" />
        </Grid.RowDefinitions>
        <Border BorderBrush="{StaticResource ContentWhiteBorderBrush}" BorderThickness="2,2,0,0">
            <Border BorderBrush="{StaticResource ContentBlackBorderBrush}" BorderThickness="0,0,2,2" regions:RegionManager.RegionName="{x:Static constant:RegionNames.ContentUpButton}" />
        </Border>
        <Border Grid.Row="1"
                BorderBrush="{StaticResource ContentWhiteBorderBrush}"
                BorderThickness="2"
                regions:RegionManager.RegionName="{x:Static constant:RegionNames.ContentDownContent}" />
    </Grid>
</UserControl>
