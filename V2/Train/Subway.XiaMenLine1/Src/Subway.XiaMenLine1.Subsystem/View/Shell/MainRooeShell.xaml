<UserControl x:Class="Subway.XiaMenLine1.Subsystem.View.Shell.MainRooeShell"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Subway.XiaMenLine1.Subsystem.View.Shell"
             xmlns:regions="http://www.codeplex.com/CompositeWPF"
             xmlns:constant="clr-namespace:Subway.XiaMenLine1.Subsystem.Constant"
             xmlns:constant1="clr-namespace:Subway.XiaMenLine1.Subsystem.Constant"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="40*" />
            <RowDefinition Height="570*" />
            <RowDefinition Height="90*" />
        </Grid.RowDefinitions>
        <ContentControl Grid.Row="0" regions:RegionManager.RegionName="{x:Static constant1:RegionNames.ShellTitleRegion}" />
        <ContentControl Grid.Row="1" regions:RegionManager.RegionName="{x:Static constant1:RegionNames.ShellContentRegion}" />
        <ContentControl Grid.Row="2" regions:RegionManager.RegionName="{x:Static constant1:RegionNames.ShellBottomRegion}" />
    </Grid>
</UserControl>
