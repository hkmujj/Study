<UserControl x:Class="Subway.XiaMenLine1.Subsystem.View.Contents.MainContentContents.CarNumber"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:viewModels="clr-namespace:Subway.XiaMenLine1.Subsystem.ViewModels"
             d:DataContext="{d:DesignInstance viewModels:ShellViewModel}"
             d:DesignHeight="10"
             d:DesignWidth="700"
             mc:Ignorable="d">
    <UserControl.Resources>
        <ResourceDictionary Source="../../../AppResouce.xaml" />
    </UserControl.Resources>
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="90*" />
            <ColumnDefinition Width="90*" />
            <ColumnDefinition Width="90*" />
            <ColumnDefinition Width="90*" />
            <ColumnDefinition Width="90*" />
            <ColumnDefinition Width="90*" />
        </Grid.ColumnDefinitions>
        <TextBlock Grid.Column="0"
                   Style="{StaticResource CarNumberTextStyle}"
                   Text="01A001" />
        <TextBlock Grid.Column="1"
                   Style="{StaticResource CarNumberTextStyle}"
                   Text="01B001" />
        <TextBlock Grid.Column="2"
                   Style="{StaticResource CarNumberTextStyle}"
                   Text="01C001" />
        <TextBlock Grid.Column="3"
                   Style="{StaticResource CarNumberTextStyle}"
                   Text="01C002" />
        <TextBlock Grid.Column="4"
                   Style="{StaticResource CarNumberTextStyle}"
                   Text="01B002" />
        <TextBlock Grid.Column="5"
                   Style="{StaticResource CarNumberTextStyle}"
                   Text="01A002" />
    </Grid>
</UserControl>
