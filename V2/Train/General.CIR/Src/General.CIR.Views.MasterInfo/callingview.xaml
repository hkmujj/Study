<?xml version="1.0" encoding="utf-8"?>
<UserControl x:Class="General.CIR.Views.MasterInfo.CallingView" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <Grid>
    <Grid.RowDefinitions>
      <RowDefinition />
      <RowDefinition />
    </Grid.RowDefinitions>
    <StackPanel Grid.Row="0" Orientation="Horizontal">
      <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center" TextBlock.FontSize="16" Text="正在呼叫" />
      <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center" TextBlock.FontSize="16" Text="{Binding MainContentViewModel.MasterInfoViewModel.CallName}" />
    </StackPanel>
    <TextBlock Grid.Row="1" HorizontalAlignment="Left" VerticalAlignment="Center" Text="请稍后" />
  </Grid>
</UserControl>