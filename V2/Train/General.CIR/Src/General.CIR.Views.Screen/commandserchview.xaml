<?xml version="1.0" encoding="utf-8"?>
<UserControl x:Class="General.CIR.Views.Screen.CommandSerchView" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <Border Background="#FF0000FF" BorderBrush="#FFA9A9A9" BorderThickness="2">
    <Grid>
      <Grid.RowDefinitions>
        <RowDefinition Height="11*" />
        <RowDefinition Height="1*" />
      </Grid.RowDefinitions>
      <ListBox Style="{StaticResource SerchViewListBoxStyle}" ItemsSource="{Binding DispatchCommandViewModel.AllSerchItems}" SelectedItem="{Binding DispatchCommandViewModel.SelectSerchItem}" />
      <TextBlock Grid.Row="1" HorizontalAlignment="Left" VerticalAlignment="Center" TextBlock.Foreground="#FF00FFFF" Text="{Binding DispatchCommandViewModel.Trips}" />
    </Grid>
  </Border>
</UserControl>