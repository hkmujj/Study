<UserControl x:Class="Subway.ShenZhenLine11.Views.CommonViews.CarView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Subway.ShenZhenLine11.Views.CommonViews"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             d:DesignHeight="100"
             d:DesignWidth="800"
             mc:Ignorable="d">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*" />
            <ColumnDefinition Width="1*" />
            <ColumnDefinition Width="1*" />
            <ColumnDefinition Width="1*" />
            <ColumnDefinition Width="1*" />
            <ColumnDefinition Width="1*" />
            <ColumnDefinition Width="1*" />
            <ColumnDefinition Width="1*" />
        </Grid.ColumnDefinitions>
        <Border Background="{StaticResource CarBackBrush}"
                BorderBrush="{StaticResource CarBorderBrush}"
                BorderThickness="0,0,2,2">
            <Border Margin="0,0,1,1"
                    Background="{StaticResource CarBackBrush}"
                    BorderBrush="{StaticResource CarBorderBrush}"
                    BorderThickness="0,0,2,2">
                <TextBlock Style="{StaticResource CarViewTextStyle}" Text="11011" />
            </Border>
        </Border>
        <Border Grid.Column="1"
                Background="{StaticResource CarBackBrush}"
                BorderBrush="{StaticResource CarBorderBrush}"
                BorderThickness="0,0,2,2">
            <Border Margin="0,0,1,1"
                    Background="{StaticResource CarBackBrush}"
                    BorderBrush="{StaticResource CarBorderBrush}"
                    BorderThickness="0,0,2,2">
                <TextBlock Style="{StaticResource CarViewTextStyle}" Text="11012" />
            </Border>
        </Border>
        <Border Grid.Column="2"
                Background="{StaticResource CarBackBrush}"
                BorderBrush="{StaticResource CarBorderBrush}"
                BorderThickness="0,0,2,2">
            <Border Margin="0,0,1,1"
                    Background="{StaticResource CarBackBrush}"
                    BorderBrush="{StaticResource CarBorderBrush}"
                    BorderThickness="0,0,2,2">
                <TextBlock Style="{StaticResource CarViewTextStyle}" Text="11013" />
            </Border>
        </Border>
        <Border Grid.Column="3"
                Background="{StaticResource CarBackBrush}"
                BorderBrush="{StaticResource CarBorderBrush}"
                BorderThickness="0,0,2,2">
            <Border Margin="0,0,1,1"
                    Background="{StaticResource CarBackBrush}"
                    BorderBrush="{StaticResource CarBorderBrush}"
                    BorderThickness="0,0,2,2">
                <TextBlock Style="{StaticResource CarViewTextStyle}" Text="11014" />
            </Border>
        </Border>
        <Border Grid.Column="4"
                Background="{StaticResource CarBackBrush}"
                BorderBrush="{StaticResource CarBorderBrush}"
                BorderThickness="0,0,2,2">
            <Border Margin="0,0,1,1"
                    Background="{StaticResource CarBackBrush}"
                    BorderBrush="{StaticResource CarBorderBrush}"
                    BorderThickness="0,0,2,2">
                <TextBlock Style="{StaticResource CarViewTextStyle}" Text="11015" />
            </Border>
        </Border>
        <Border Grid.Column="5"
                Background="{StaticResource CarBackBrush}"
                BorderBrush="{StaticResource CarBorderBrush}"
                BorderThickness="0,0,2,2">
            <Border Margin="0,0,1,1"
                    Background="{StaticResource CarBackBrush}"
                    BorderBrush="{StaticResource CarBorderBrush}"
                    BorderThickness="0,0,2,2">
                <TextBlock Style="{StaticResource CarViewTextStyle}" Text="11016" />
            </Border>
        </Border>
        <Border Grid.Column="6"
                Background="{StaticResource CarBackBrush}"
                BorderBrush="{StaticResource CarBorderBrush}"
                BorderThickness="0,0,2,2">
            <Border Margin="0,0,1,1"
                    Background="{StaticResource CarBackBrush}"
                    BorderBrush="{StaticResource CarBorderBrush}"
                    BorderThickness="0,0,2,2">
                <TextBlock Style="{StaticResource CarViewTextStyle}" Text="11017" />
            </Border>
        </Border>
        <Border Grid.Column="7"
                Background="{StaticResource CarBackBrush}"
                BorderBrush="{StaticResource CarBorderBrush}"
                BorderThickness="0,0,2,2">
            <Border Margin="0,0,1,1"
                    Background="{StaticResource CarBackBrush}"
                    BorderBrush="{StaticResource CarBorderBrush}"
                    BorderThickness="0,0,2,2">
                <TextBlock Style="{StaticResource CarViewTextStyle}" Text="11018" />
            </Border>
        </Border>
    </Grid>
</UserControl>
