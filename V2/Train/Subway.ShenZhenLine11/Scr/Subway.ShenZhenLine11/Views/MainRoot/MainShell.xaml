<UserControl x:Class="Subway.ShenZhenLine11.Views.MainRoot.MainShell"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:name="clr-namespace:Subway.ShenZhenLine11.Constance"
             xmlns:region="http://www.codeplex.com/CompositeWPF"
             d:DesignHeight="600"
             d:DesignWidth="800"
             mc:Ignorable="d">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="50*" />
            <RowDefinition Height="555*" />
            <RowDefinition Height="65*" />
        </Grid.RowDefinitions>
        <ContentControl Grid.Row="0" region:RegionManager.RegionName="{x:Static name:RegionNames.Title}" />
        <ContentControl Grid.Row="1" region:RegionManager.RegionName="{x:Static name:RegionNames.MainContent}" />
        <ContentControl Grid.Row="2" region:RegionManager.RegionName="{x:Static name:RegionNames.BottomButton}" />
    </Grid>
</UserControl>
