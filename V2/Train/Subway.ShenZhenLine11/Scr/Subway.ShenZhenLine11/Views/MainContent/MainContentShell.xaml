<UserControl x:Class="Subway.ShenZhenLine11.Views.MainContent.MainContentShell"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:name="clr-namespace:Subway.ShenZhenLine11.Constance"
             xmlns:regin="http://www.codeplex.com/CompositeWPF"
             d:DesignHeight="333"
             d:DesignWidth="538"
             mc:Ignorable="d">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="70*" />
            <RowDefinition Height="485*" />
        </Grid.RowDefinitions>

        <ContentControl Grid.Row="0" regin:RegionManager.RegionName="{x:Static name:RegionNames.BaseInfo}" />
        <ContentControl Grid.Row="1"
                        Margin="20,5,20,0"
                        regin:RegionManager.RegionName="{x:Static name:RegionNames.MainMaster}" />
    </Grid>
</UserControl>
