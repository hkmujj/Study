<UserControl x:Class="Subway.TCMS.LanZhou.View.Shell.ShellWithoutButton"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:constant="clr-namespace:Subway.TCMS.LanZhou.Constant"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:regions="http://www.codeplex.com/CompositeWPF"
             Background="{StaticResource Subway.TCMS.LanZhou.Brush.ContentBackground}"
             d:DesignHeight="600"
             d:DesignWidth="800"
             mc:Ignorable="d">
    <Viewbox Stretch="Fill">
        <Border Width="800" Height="600" regions:RegionManager.RegionName="{x:Static constant:RegionNames.ShellContent}" />
    </Viewbox>
    
</UserControl>
