<UserControl x:Class="Subway.TCMS.LanZhou.View.Contents.BreakDownInformation.BreakDownHistroyList"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Subway.TCMS.LanZhou.View.Contents.BreakDownInformation"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             Background="{StaticResource Subway.TCMS.LanZhou.Brush.ContentBackground}"
             d:DesignHeight="466"
             d:DesignWidth="800"
             mc:Ignorable="d">
    <Grid>
        <!--<Grid.Background>
            <ImageBrush ImageSource="{StaticResource Subway.TCMS.LanZhou.ImgTemporaryBreakDownHistroyList}" />
        </Grid.Background>-->
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="52*" />
                <RowDefinition Height="360*" />
                <RowDefinition Height="10*" />
                <RowDefinition Height="39*" />
                <RowDefinition Height="5*" />
            </Grid.RowDefinitions>
            <Grid Row="0">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="47*" />
                    <ColumnDefinition Width="48*" />
                    <ColumnDefinition Width="49*" />
                    <ColumnDefinition Width="40*" />
                    <ColumnDefinition Width="279*" />
                    <ColumnDefinition Width="167*" />
                    <ColumnDefinition Width="170*" />
                </Grid.ColumnDefinitions>
                <Border Grid.Column="0"
                        Margin="0,0,-1,0"
                        Background="{StaticResource Subway.TCMS.LanZhou.Brush.TextWrapStyleBackground}"
                        BorderBrush="{StaticResource Subway.TCMS.LanZhou.Brush.ForegroundWhite}"
                        BorderThickness="2">
                    <TextBlock HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               FontSize="17"
                               Foreground="{StaticResource Subway.TCMS.LanZhou.Brush.ForegroundWhite}"
                               Text="{StaticResource Subway.TCMS.LanZhou.String.SerialNumber}" />
                </Border>
                <Border Grid.Column="1"
                        Margin="0,0,-1,0"
                        Background="{StaticResource Subway.TCMS.LanZhou.Brush.TextWrapStyleBackground}"
                        BorderBrush="{StaticResource Subway.TCMS.LanZhou.Brush.ForegroundWhite}"
                        BorderThickness="2">
                    <TextBlock HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               FontSize="17"
                               Foreground="{StaticResource Subway.TCMS.LanZhou.Brush.ForegroundWhite}"
                               Text="{StaticResource Subway.TCMS.LanZhou.String.FaultNumber}"
                               TextWrapping="Wrap" />
                </Border>
                <Border Grid.Column="2"
                        Margin="0,0,-1,0"
                        Background="{StaticResource Subway.TCMS.LanZhou.Brush.TextWrapStyleBackground}"
                        BorderBrush="{StaticResource Subway.TCMS.LanZhou.Brush.ForegroundWhite}"
                        BorderThickness="2">
                    <TextBlock Width="50"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               FontSize="17"
                               Foreground="{StaticResource Subway.TCMS.LanZhou.Brush.ForegroundWhite}"
                               Text="{StaticResource Subway.TCMS.LanZhou.String.FaultCode}"
                               TextAlignment="Center"
                               TextWrapping="Wrap" />
                </Border>
                <Border Grid.Column="3"
                        Margin="0,0,-1,0"
                        Background="{StaticResource Subway.TCMS.LanZhou.Brush.TextWrapStyleBackground}"
                        BorderBrush="{StaticResource Subway.TCMS.LanZhou.Brush.ForegroundWhite}"
                        BorderThickness="2">
                    <TextBlock HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               FontSize="17"
                               Foreground="{StaticResource Subway.TCMS.LanZhou.Brush.ForegroundWhite}"
                               Text="{StaticResource Subway.TCMS.LanZhou.String.Grade}"
                               TextAlignment="Center" />
                </Border>
                <Border Grid.Column="4"
                        Margin="0,0,-1,0"
                        Background="{StaticResource Subway.TCMS.LanZhou.Brush.TextWrapStyleBackground}"
                        BorderBrush="{StaticResource Subway.TCMS.LanZhou.Brush.ForegroundWhite}"
                        BorderThickness="2">
                    <TextBlock HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               FontSize="17"
                               Foreground="{StaticResource Subway.TCMS.LanZhou.Brush.ForegroundWhite}"
                               Text="{StaticResource Subway.TCMS.LanZhou.String.FaultDescription}" />
                </Border>
                <Border Grid.Column="5"
                        Margin="0,0,-1,0"
                        Background="{StaticResource Subway.TCMS.LanZhou.Brush.TextWrapStyleBackground}"
                        BorderBrush="{StaticResource Subway.TCMS.LanZhou.Brush.ForegroundWhite}"
                        BorderThickness="2">
                    <TextBlock HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               FontSize="17"
                               Foreground="{StaticResource Subway.TCMS.LanZhou.Brush.ForegroundWhite}"
                               Text="{StaticResource Subway.TCMS.LanZhou.String.DateforFailure}" />
                </Border>
                <Border Grid.Column="6"
                        Margin="0,0,-1,0"
                        Background="{StaticResource Subway.TCMS.LanZhou.Brush.TextWrapStyleBackground}"
                        BorderBrush="{StaticResource Subway.TCMS.LanZhou.Brush.ForegroundWhite}"
                        BorderThickness="2">
                    <TextBlock HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               FontSize="17"
                               Foreground="{StaticResource Subway.TCMS.LanZhou.Brush.ForegroundWhite}"
                               Text="{StaticResource Subway.TCMS.LanZhou.String.EndDateforFailure}" />
                </Border>
            </Grid>
            <Grid Row="1">
                <Border Margin="0,-1,0,0"
                        BorderBrush="{StaticResource Subway.TCMS.LanZhou.Brush.ForegroundWhite}"
                        BorderThickness="2" />
            </Grid>
            <Grid Row="3">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="579*" />
                    <ColumnDefinition Width="69*" />
                    <ColumnDefinition Width="5*" />
                    <ColumnDefinition Width="68*" />
                    <ColumnDefinition Width="5*" />
                    <ColumnDefinition Width="68*" />
                    <ColumnDefinition Width="6*" />
                </Grid.ColumnDefinitions>
                <Grid Column="0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="19*" />
                        <ColumnDefinition Width="79*" />
                        <ColumnDefinition Width="41*" />
                        <ColumnDefinition Width="34*" />
                        <ColumnDefinition Width="79*" />
                        <ColumnDefinition Width="24*" />
                        <ColumnDefinition Width="33*" />
                        <ColumnDefinition Width="80*" />
                        <ColumnDefinition Width="32*" />
                        <ColumnDefinition Width="158*" />
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition />
                        <RowDefinition Height="2*" />
                    </Grid.RowDefinitions>
                    <TextBlock Grid.Row="1"
                               Grid.Column="1"
                               Margin="5,1,0,0"
                               FontSize="16"
                               Foreground="{StaticResource Subway.TCMS.LanZhou.Brush.ForegroundWhite}"
                               Text="{StaticResource Subway.TCMS.LanZhou.String.TotalFault}" />
                    <TextBlock Grid.Row="1"
                               Grid.Column="4"
                               Margin="5,1,0,0"
                               FontSize="16"
                               Foreground="{StaticResource Subway.TCMS.LanZhou.Brush.ForegroundWhite}"
                               Text="{StaticResource Subway.TCMS.LanZhou.String.CurrentPage}" />
                    <TextBlock Grid.Row="1"
                               Grid.Column="7"
                               Margin="5,1,0,0"
                               FontSize="16"
                               Foreground="{StaticResource Subway.TCMS.LanZhou.Brush.ForegroundWhite}"
                               Text="{StaticResource Subway.TCMS.LanZhou.String.TotalPages}" />

                </Grid>
                <Button Grid.Row="0"
                        Grid.Column="1"
                        Width="68"
                        Height="39"
                        Content="{StaticResource Subway.TCMS.LanZhou.String.PreviousPage}"
                        Style="{StaticResource Subway.TCMS.LanZhou.HardwareBtnStyle}" />
                <Button Grid.Row="0"
                        Grid.Column="3"
                        Width="68"
                        Height="39"
                        Content="{StaticResource Subway.TCMS.LanZhou.String.NextPage}"
                        Style="{StaticResource Subway.TCMS.LanZhou.HardwareBtnStyle}" />
                <Button Grid.Row="0"
                        Grid.Column="5"
                        Width="68"
                        Height="39"
                        Content="{StaticResource Subway.TCMS.LanZhou.String.HomePage}"
                        Style="{StaticResource Subway.TCMS.LanZhou.HardwareBtnStyle}" />

            </Grid>


        </Grid>
    </Grid>
</UserControl>
