<UserControl x:Class="Subway.TCMS.LanZhou.View.Contents.BreakDownInformation.BreakDownInformationDetail"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Subway.TCMS.LanZhou.View.Contents.BreakDownInformation"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             d:DesignHeight="466"
             d:DesignWidth="800"
             Background="{StaticResource Subway.TCMS.LanZhou.Brush.ContentBackground}"
             mc:Ignorable="d">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="{StaticResource Subway.TCMS.LanZhou.ImgTemporaryFaultDetailView}" Stretch="Uniform" />
        </Grid.Background>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="13*" />
            <ColumnDefinition Width="773*" />
            <ColumnDefinition Width="14*" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="51*" />
            <RowDefinition Height="358*" />
            <RowDefinition Height="57*" />
        </Grid.RowDefinitions>
        <Border Grid.Row="1"
                Grid.Column="1"
                BorderBrush="{StaticResource Subway.TCMS.LanZhou.Brush.ForegroundWhite}"
                BorderThickness="1">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="48*" />
                    <RowDefinition Height="48*" />
                    <RowDefinition Height="48*" />
                    <RowDefinition Height="48*" />
                    <RowDefinition Height="48*" />
                    <RowDefinition Height="130*" />
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="181*" />
                    <ColumnDefinition Width="592*" />
                </Grid.ColumnDefinitions>
                <TextBox Grid.Row="0"
                         Grid.Column="0"
                         Style="{StaticResource Subway.TCMS.LanZhou.FualtDetailPageBorderTextStyle}"
                         Text="{StaticResource Subway.TCMS.LanZhou.String.FaultLevel}" />
                <TextBox Grid.Row="0"
                         Grid.Column="1"
                         Style="{StaticResource Subway.TCMS.LanZhou.FualtDetailPageNoBackgroundBorderTextStyle}"
                         Text="1" />
                <TextBox Grid.Row="1"
                         Grid.Column="0"
                         Style="{StaticResource Subway.TCMS.LanZhou.FualtDetailPageBorderTextStyle}"
                         Text="{StaticResource Subway.TCMS.LanZhou.String.FaultCode}" />
                <TextBox Grid.Row="1"
                         Grid.Column="1"
                         Style="{StaticResource Subway.TCMS.LanZhou.FualtDetailPageNoBackgroundBorderTextStyle}"
                         Text="1" />
                <TextBox Grid.Row="2"
                         Grid.Column="0"
                         Style="{StaticResource Subway.TCMS.LanZhou.FualtDetailPageBorderTextStyle}"
                         Text="{StaticResource Subway.TCMS.LanZhou.String.FaultLocation}" />
                <TextBox Grid.Row="2"
                         Grid.Column="1"
                         Style="{StaticResource Subway.TCMS.LanZhou.FualtDetailPageNoBackgroundBorderTextStyle}"
                         Text="1" />
                <TextBox Grid.Row="3"
                         Grid.Column="0"
                         Style="{StaticResource Subway.TCMS.LanZhou.FualtDetailPageBorderTextStyle}"
                         Text="{StaticResource Subway.TCMS.LanZhou.String.OccurrenceTime}" />
                <TextBox Grid.Row="3"
                         Grid.Column="1"
                         Style="{StaticResource Subway.TCMS.LanZhou.FualtDetailPageNoBackgroundBorderTextStyle}"
                         Text="1" />
                <TextBox Grid.Row="4"
                         Grid.Column="0"
                         Style="{StaticResource Subway.TCMS.LanZhou.FualtDetailPageBorderTextStyle}"
                         Text="{StaticResource Subway.TCMS.LanZhou.String.FaultDescription}" />
                <TextBox Grid.Row="4"
                         Grid.Column="1"
                         Style="{StaticResource Subway.TCMS.LanZhou.FualtDetailPageNoBackgroundBorderTextStyle}"
                         Text="1" />
                <TextBox Grid.Row="5"
                         Grid.Column="0"
                         Style="{StaticResource Subway.TCMS.LanZhou.FualtDetailPageBorderTextStyle}"
                         Text="{StaticResource Subway.TCMS.LanZhou.String.DriverTips}" />
                <TextBox Grid.Row="5"
                         Grid.Column="1"
                         Style="{StaticResource Subway.TCMS.LanZhou.FualtDetailPageNoBackgroundBorderTextStyle}"
                         Text="1" />
            </Grid>
        </Border>

    </Grid>
</UserControl>
