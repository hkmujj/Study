    <Style x:Key="Subway.TCMS.LanZhou.TrainBodyStatusItemStyle" TargetType="ItemsControl">
        <Style.Setters>
            <Setter Property="ItemsPanel">
                <Setter.Value>
                    <ItemsPanelTemplate>
                        <StackPanel HorizontalAlignment="Stretch" Orientation="Horizontal" />
                    </ItemsPanelTemplate>
                </Setter.Value>
            </Setter>
            <Setter Property="ItemTemplate">
                <Setter.Value>
                    <DataTemplate DataType="train:CarModel">
                        <Border Width="{Binding RelativeSource={RelativeSource AncestorType=ItemsControl,
                                                                               Mode=FindAncestor},
                                                Path=ActualWidth,
                                                Converter={StaticResource MathConverter},
                                                ConverterParameter=x/6}"
                                BorderBrush="{StaticResource Subway.TCMS.LanZhou.Brush.ForegroundWhite}"
                                BorderThickness="2">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*" />
                                    <RowDefinition Height="*" />
                                    <RowDefinition Height="0.3*" />
                                    <RowDefinition Height="*" />
                                    <RowDefinition Height="0.5*" />
                                    <RowDefinition Height="2*" />
                                    <RowDefinition Height="0.5*" />
                                    <RowDefinition Height="2*" />
                                    <RowDefinition Height="0.5*" />
                                    <RowDefinition Height="*" />
                                    <RowDefinition Height="0.3*" />
                                    <RowDefinition Height="*" />
                                    <RowDefinition Height="*" />
                                </Grid.RowDefinitions>

                                <Grid Row="7">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="15*" />
                                        <ColumnDefinition Width="20*" />
                                        <ColumnDefinition Width="3*" />
                                        <ColumnDefinition Width="20*" />
                                        <ColumnDefinition Width="15*" />
                                    </Grid.ColumnDefinitions>
                                    <Image Grid.Column="1"
                                           Source="{Binding Path=TrainBodyViewData.AuxiliarySystemStatus.State,
                                                            Converter={StaticResource Subway.TCMS.LanZhou.Converter.AuxiliarySystemImageConverter}}"
                                           Visibility="{Binding Path=TrainBodyViewData.AuxiliarySystemStatus.Visible,
                                                                Converter={StaticResource BoolVisibilityConverter}}" />
                                    <Image Grid.Column="3"
                                           Source="{Binding Path=TrainBodyViewData.AirCompressorStatus.State,
                                                            Converter={StaticResource Subway.TCMS.LanZhou.Converter.AirCompressorImageConverter}}"
                                           Visibility="{Binding Path=TrainBodyViewData.AirCompressorStatus.Visible,
                                                                Converter={StaticResource BoolVisibilityConverter}}" />
                                </Grid>

                                <Grid Row="9">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*" />
                                        <ColumnDefinition Width="*" />
                                        <ColumnDefinition Width="*" />
                                    </Grid.ColumnDefinitions>
                                    <Image Grid.Column="0"
                                           Source="{Binding Path=TrainBodyViewData.Brake1Status.State,
                                                            Converter={StaticResource Subway.TCMS.LanZhou.Converter.CommonBrakeImageConverter}}"
                                           Visibility="{Binding Path=TrainBodyViewData.Brake1Status.Visible,
                                                                Converter={StaticResource BoolVisibilityConverter}}" />
                                    <Image Grid.Column="1"
                                           Source="{Binding Path=TrainBodyViewData.ParkingBrakeStatus.State,
                                                            Converter={StaticResource Subway.TCMS.LanZhou.Converter.ParkingBrakeImageConverter}}"
                                           Visibility="{Binding Path=TrainBodyViewData.ParkingBrakeStatus.Visible,
                                                                Converter={StaticResource BoolVisibilityConverter}}" />
                                    <Image Grid.Column="2"
                                           Source="{Binding Path=TrainBodyViewData.Brake2Status.State,
                                                            Converter={StaticResource Subway.TCMS.LanZhou.Converter.CommonBrakeImageConverter}}"
                                           Visibility="{Binding Path=TrainBodyViewData.Brake2Status.Visible,
                                                                Converter={StaticResource BoolVisibilityConverter}}" />
                                </Grid>
                                <ItemsControl Grid.Row="0"
                                              ItemsSource="{Binding Path=DoorModel.UpDoors}"
                                              Style="{StaticResource Subway.TCMS.LanZhou.CarDoorIndexStyle}" />
                                <ItemsControl Grid.Row="1"
                                              ItemsSource="{Binding Path=DoorModel.UpDoors}"
                                              Style="{StaticResource Subway.TCMS.LanZhou.CarDoorItemStyle}" />
                                <TextBlock Grid.Row="3"
                                           HorizontalAlignment="Center"
                                           VerticalAlignment="Center"
                                           FontFamily="{StaticResource FontFamilySongTypeface}"
                                           FontSize="20"
                                           FontWeight="Bold"
                                           Foreground="{StaticResource Subway.TCMS.LanZhou.Brush.ForegroundWhite}"
                                           Text="{Binding Path=Name}" />
                                <Image Grid.Row="5"
                                       HorizontalAlignment="Center"
                                       Source="{Binding Path=TrainBodyViewData.TowState.State,
                                                        Converter={StaticResource Subway.TCMS.LanZhou.Converter.TractionSystemImageConverter}}"
                                       Visibility="{Binding Path=TrainBodyViewData.TowState.Visible,
                                                            Converter={StaticResource BoolVisibilityConverter}}" />
                                <ItemsControl Grid.Row="11"
                                              ItemsSource="{Binding Path=DoorModel.DownDoors}"
                                              Style="{StaticResource Subway.TCMS.LanZhou.CarDoorItemStyle}" />

                                <ItemsControl Grid.Row="12"
                                              ItemsSource="{Binding Path=DoorModel.DownDoors}"
                                              Style="{StaticResource Subway.TCMS.LanZhou.CarDoorIndexStyle}" />


                            </Grid>
                        </Border>
                    </DataTemplate>
                </Setter.Value>
            </Setter>
        </Style.Setters>
    </Style>

