<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:carPart="clr-namespace:Subway.TCMS.LanZhou.Model.Domain.Car.CarParts"
                    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                    xmlns:fault="clr-namespace:Subway.TCMS.LanZhou.Model.Domain.Fault"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:train="clr-namespace:Subway.TCMS.LanZhou.Model.Domain.Car"
                    xmlns:view="clr-namespace:MMI.Facility.WPFInfrastructure.View;assembly=MMI.Facility.WPFInfrastructure"
                    mc:Ignorable="d">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="../Base/TCMSLanZhouBaseResource.xaml" />
        <ResourceDictionary Source="../Image/TCMSLanZhouImageResource.xaml" />
        <ResourceDictionary Source="../Converter/TCMSLanZhouConverterResources.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <Style x:Key="Subway.TCMS.LanZhou.BrakePressureItemStyle" TargetType="ItemsControl">
        <Style.Setters>
            <Setter Property="ItemsPanel">
                <Setter.Value>
                    <ItemsPanelTemplate>
                        <StackPanel HorizontalAlignment="Stretch" Orientation="Horizontal" />
                    </ItemsPanelTemplate>
                </Setter.Value>
            </Setter>
            <Setter Property="ItemTemplate">
                <Setter.Value>
                    <DataTemplate DataType="train:CarModel">
                        <Grid>
                            <Grid.Width>
                                <MultiBinding Converter="{StaticResource MathConverter}" ConverterParameter="x/6">
                                    <Binding Path="ActualWidth" RelativeSource="{RelativeSource AncestorType=ItemsControl, Mode=FindAncestor}" />
                                </MultiBinding>
                            </Grid.Width>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="1*" />
                                <ColumnDefinition Width="1*" />
                            </Grid.ColumnDefinitions>
                            <Grid Grid.Column="0" Visibility="{Binding Path=RunningViewData.RunningViewFloatDataLazy.Value.BrakePressure1.Visible, Converter={StaticResource BoolVisibilityConverter}}">
                                <Border Margin="-1,0,0,0"
                                        Background="{StaticResource Subway.TCMS.LanZhou.Brush.ContentBackground}"
                                        BorderBrush="{StaticResource Subway.TCMS.LanZhou.Brush.ForegroundWhite}"
                                        BorderThickness="1">
                                    <TextBlock VerticalAlignment="Center "
                                               FontFamily="{StaticResource FontFamilySongTypeface}"
                                               FontSize="18"
                                               Foreground="{StaticResource Subway.TCMS.LanZhou.Brush.ForegroundWhite}"
                                               Text="{Binding Path=RunningViewData.RunningViewFloatDataLazy.Value.BrakePressure1.State.Value,
                                                              StringFormat={}0}"
                                               TextAlignment="Center" />
                                </Border>

                                <Image Source="{StaticResource Subway.TCMS.LanZhou.ImgCommonUnKnowStatusBackSmall}"
                                       Stretch="UniformToFill"
                                       Visibility="{Binding Path=RunningViewData.RunningViewFloatDataLazy.Value.BrakePressure1.State.GeneralState,
                                                            Converter={StaticResource BoolVisibilityConverter}}" />
                            </Grid>
                            <Grid Grid.Column="1" Visibility="{Binding Path=RunningViewData.RunningViewFloatDataLazy.Value.BrakePressure2.Visible, Converter={StaticResource BoolVisibilityConverter}}">
                                <Border Margin="-1,0,0,0"
                                        Background="{StaticResource Subway.TCMS.LanZhou.Brush.ContentBackground}"
                                        BorderBrush="{StaticResource Subway.TCMS.LanZhou.Brush.ForegroundWhite}"
                                        BorderThickness="1">
                                    <TextBlock VerticalAlignment="Center "
                                               FontFamily="{StaticResource FontFamilySongTypeface}"
                                               FontSize="18"
                                               Foreground="{StaticResource Subway.TCMS.LanZhou.Brush.ForegroundWhite}"
                                               Text="{Binding Path=RunningViewData.RunningViewFloatDataLazy.Value.BrakePressure2.State.Value,
                                                              StringFormat={}0}"
                                               TextAlignment="Center" />
                                </Border>
                                <Image Source="{StaticResource Subway.TCMS.LanZhou.ImgCommonUnKnowStatusBackSmall}"
                                       Stretch="UniformToFill"
                                       Visibility="{Binding Path=RunningViewData.RunningViewFloatDataLazy.Value.BrakePressure2.State.GeneralState,
                                                            Converter={StaticResource BoolVisibilityConverter}}" />
                            </Grid>
                        </Grid>
                    </DataTemplate>
                </Setter.Value>
            </Setter>
        </Style.Setters>
    </Style>

