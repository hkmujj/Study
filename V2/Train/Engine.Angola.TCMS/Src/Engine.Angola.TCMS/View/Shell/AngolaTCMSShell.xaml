<Window x:Class="Engine.Angola.TCMS.View.Shell.AngolaTCMSShell"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:constant="clr-namespace:Engine.Angola.TCMS.Constant"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:regions="http://www.codeplex.com/CompositeWPF"
        xmlns:viewModel="clr-namespace:Engine.Angola.TCMS.ViewModel"
        Title="Angola.TCMS"
        Width="960"
        Height="720"
        Loaded="AngolaTCMSShell_OnLoaded"
        d:DataContext="{d:DesignInstance viewModel:AngolaTCMSShellViewModel}"
        mc:Ignorable="d">
    <Border regions:RegionManager.RegionName="{x:Static constant:RegionNames.ShellRegion}" />
</Window>
