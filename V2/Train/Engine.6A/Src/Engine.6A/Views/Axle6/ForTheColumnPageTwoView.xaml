<UserControl x:Class="Engine._6A.Views.Axle6.ForTheColumnPageTwoView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:commonControl="clr-namespace:Engine._6A.CommonControl"
             xmlns:viewModel="clr-namespace:Engine._6A.Interface.ViewModel" d:DataContext="{d:DesignInstance viewModel:IEngine6AViewModel}"
             d:DesignHeight="400"
             d:DesignWidth="510"
             mc:Ignorable="d">
    <UserControl.Resources>
        <ResourceDictionary Source="../../Resouce/Engine6AResource.xaml" />
    </UserControl.Resources>
    <Grid Margin="5" IsEnabled="{Binding DataMonitor.ForTheColumn.ForTheColumnTwo}">
        <Grid Grid.Column="0">
            <Grid Margin="0">
                <Grid.RowDefinitions>
                    <RowDefinition Height="345*" />
                </Grid.RowDefinitions>
                <Grid Grid.Row="0">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="1*" />
                        <RowDefinition Height="1*" />
                        <RowDefinition Height="1*" />
                        <RowDefinition Height="1*" />
                        <RowDefinition Height="1*" />
                        <RowDefinition Height="1*" />
                        <RowDefinition Height="1*" />
                        <RowDefinition Height="1*" />
                        <RowDefinition Height="1*" />
                        <RowDefinition Height="1*" />
                        <RowDefinition Height="1*" />
                        <RowDefinition Height="1*" />
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="10*" />
                        <ColumnDefinition Width="4*" />
                        <ColumnDefinition Width="4*" />
                        <ColumnDefinition Width="10*" />
                        <ColumnDefinition Width="4*" />
                    </Grid.ColumnDefinitions>
                    <TextBlock Grid.Row="0"
                               Grid.Column="1"
                               Style="{StaticResource CommonTextStyleOne}"
                               Text="1路" />
                    <TextBlock Grid.Row="0"
                               Grid.Column="2"
                               Style="{StaticResource CommonTextStyleOne}"
                               Text="2路" />
                    <TextBlock Grid.Row="0"
                               Grid.Column="4"
                               Style="{StaticResource CommonTextStyleOne}"
                               Text="其他" />
                    <TextBlock Grid.Row="1"
                               Grid.Column="0"
                               Style="{StaticResource CommonTextStyleTwo}"
                               Text="供电申请" />

                    <commonControl:RadioRecTest Grid.Row="1"
                                             Grid.Column="1"
                                             Style="{StaticResource ForTheColumnViewBoxStyleTwo}" />
                    <commonControl:RadioRecTest Grid.Row="1"
                                             Grid.Column="2"
                                             Style="{StaticResource ForTheColumnViewBoxStyleTwo}" />


                    <TextBlock Grid.Row="1"
                               Grid.Column="3"
                               Style="{StaticResource CommonTextStyleTwo}"
                               Text="1端占用" />
                    <commonControl:RadioRecTest Grid.Row="1"
                                             Grid.Column="4"
                                             Style="{StaticResource ForTheColumnViewBoxStyleTwo}" />
                    <TextBlock Grid.Row="2"
                               Grid.Column="0"
                               Style="{StaticResource CommonTextStyleTwo}"
                               Text="供电钥匙闭合" />
                    <commonControl:RadioRecTest Grid.Row="2"
                                             Grid.Column="1"
                                             Style="{StaticResource ForTheColumnViewBoxStyleTwo}" />
                    <commonControl:RadioRecTest Grid.Row="2"
                                             Grid.Column="2"
                                             Style="{StaticResource ForTheColumnViewBoxStyleTwo}" />
                    <TextBlock Grid.Row="2"
                               Grid.Column="3"
                               Style="{StaticResource CommonTextStyleTwo}"
                               Text="2端占用" />
                    <commonControl:RadioRecTest Grid.Row="2"
                                             Grid.Column="4"
                                             Style="{StaticResource ForTheColumnViewBoxStyleTwo}" />
                    <TextBlock Grid.Row="3"
                               Grid.Column="0"
                               Style="{StaticResource CommonTextStyleTwo}"
                               Text="客车电源有效" />
                    <commonControl:RadioRecTest Grid.Row="3"
                                             Grid.Column="1"
                                             Style="{StaticResource ForTheColumnViewBoxStyleTwo}" />
                    <commonControl:RadioRecTest Grid.Row="3"
                                             Grid.Column="2"
                                             Style="{StaticResource ForTheColumnViewBoxStyleTwo}" />
                    <TextBlock Grid.Row="3"
                               Grid.Column="3"
                               Style="{StaticResource CommonTextStyleTwo}"
                               Text="1端电钥匙闭合" />
                    <commonControl:RadioRecTest Grid.Row="3"
                                             Grid.Column="4"
                                             Style="{StaticResource ForTheColumnViewBoxStyleTwo}" />
                    <TextBlock Grid.Row="4"
                               Grid.Column="0"
                               Style="{StaticResource CommonTextStyleTwo}"
                               Text="供电允许" />
                    <commonControl:RadioRecTest Grid.Row="4"
                                             Grid.Column="1"
                                             Style="{StaticResource ForTheColumnViewBoxStyleTwo}" />
                    <commonControl:RadioRecTest Grid.Row="4"
                                             Grid.Column="2"
                                             Style="{StaticResource ForTheColumnViewBoxStyleTwo}" />
                    <TextBlock Grid.Row="4"
                               Grid.Column="3"
                               Style="{StaticResource CommonTextStyleTwo}"
                               Text="2端电钥匙闭合" />
                    <commonControl:RadioRecTest Grid.Row="4"
                                             Grid.Column="4"
                                             Style="{StaticResource ForTheColumnViewBoxStyleTwo}" />
                    <TextBlock Grid.Row="5"
                               Grid.Column="0"
                               Style="{StaticResource CommonTextStyleTwo}"
                               Text="A组运行" />
                    <commonControl:RadioRecTest Grid.Row="5"
                                             Grid.Column="1"
                                             Style="{StaticResource ForTheColumnViewBoxStyleTwo}" />
                    <commonControl:RadioRecTest Grid.Row="5"
                                             Grid.Column="2"
                                             Style="{StaticResource ForTheColumnViewBoxStyleTwo}" />
                    <TextBlock Grid.Row="5"
                               Grid.Column="3"
                               Style="{StaticResource CommonTextStyleTwo}"
                               Text="辅变运行" />
                    <commonControl:RadioRecTest Grid.Row="5"
                                             Grid.Column="4"
                                             Style="{StaticResource ForTheColumnViewBoxStyleTwo}" />
                    <TextBlock Grid.Row="6"
                               Grid.Column="0"
                               Style="{StaticResource CommonTextStyleTwo}"
                               Text="B组运行" />
                    <commonControl:RadioRecTest Grid.Row="6"
                                             Grid.Column="1"
                                             Style="{StaticResource ForTheColumnViewBoxStyleTwo}" />
                    <commonControl:RadioRecTest Grid.Row="6"
                                             Grid.Column="2"
                                             Style="{StaticResource ForTheColumnViewBoxStyleTwo}" />
                    <TextBlock Grid.Row="6"
                               Grid.Column="3"
                               Style="{StaticResource CommonTextStyleTwo}"
                               Text="主断闭合" />
                    <commonControl:RadioRecTest Grid.Row="6"
                                             Grid.Column="4"
                                             Style="{StaticResource ForTheColumnViewBoxStyleTwo}" />
                    <TextBlock Grid.Row="7"
                               Grid.Column="0"
                               Style="{StaticResource CommonTextStyleTwo}"
                               Text="接地隔离开关闭合" />
                    <commonControl:RadioRecTest Grid.Row="7"
                                             Grid.Column="1"
                                             Style="{StaticResource ForTheColumnViewBoxStyleTwo}" />
                    <commonControl:RadioRecTest Grid.Row="7"
                                             Grid.Column="2"
                                             Style="{StaticResource ForTheColumnViewBoxStyleTwo}" />
                    <TextBlock Grid.Row="7"
                               Grid.Column="3"
                               Style="{StaticResource CommonTextStyleTwo}"
                               Text="列供风机允许" />
                    <commonControl:RadioRecTest Grid.Row="7"
                                             Grid.Column="4"
                                             Style="{StaticResource ForTheColumnViewBoxStyleTwo}" />
                    <TextBlock Grid.Row="8"
                               Grid.Column="0"
                               Style="{StaticResource CommonTextStyleTwo}"
                               Text="集控隔离开关闭合" />
                    <commonControl:RadioRecTest Grid.Row="8"
                                             Grid.Column="1"
                                             Style="{StaticResource ForTheColumnViewBoxStyleTwo}" />
                    <commonControl:RadioRecTest Grid.Row="8"
                                             Grid.Column="2"
                                             Style="{StaticResource ForTheColumnViewBoxStyleTwo}" />
                    <TextBlock Grid.Row="9"
                               Grid.Column="0"
                               Style="{StaticResource CommonTextStyleTwo}"
                               Text="温度继电器1动作" />
                    <commonControl:RadioRecTest Grid.Row="9"
                                             Grid.Column="1"
                                             Style="{StaticResource ForTheColumnViewBoxStyleTwo}" />
                    <commonControl:RadioRecTest Grid.Row="9"
                                             Grid.Column="2"
                                             Style="{StaticResource ForTheColumnViewBoxStyleTwo}" />
                    <TextBlock Grid.Row="10"
                               Grid.Column="0"
                               Style="{StaticResource CommonTextStyleTwo}"
                               Text="温度继电器2动作" />
                    <commonControl:RadioRecTest Grid.Row="10"
                                             Grid.Column="1"
                                             Style="{StaticResource ForTheColumnViewBoxStyleTwo}" />
                    <commonControl:RadioRecTest Grid.Row="10"
                                             Grid.Column="2"
                                             Style="{StaticResource ForTheColumnViewBoxStyleTwo}" />
                </Grid>
            </Grid>
        </Grid>
    </Grid>
</UserControl>
