    <DrawingBrush x:Key="SystemTextBorderBrush">
        <DrawingBrush.Drawing>
            <GeometryDrawing>
                <GeometryDrawing.Pen>
                    <Pen Brush="{StaticResource AquaBrush}" Thickness="1">
                        <Pen.DashStyle>
                            <DashStyle Dashes="1,1,1,1" />
                        </Pen.DashStyle>
                    </Pen>
                </GeometryDrawing.Pen>
            </GeometryDrawing>
        </DrawingBrush.Drawing>
    </DrawingBrush>

