<UserControl x:Class="Urban.ATC.CommonView.WPFControl.RegionD.Menu"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:model="clr-namespace:Urban.ATC.Siemens.Model.ViewModel;assembly=Urban.ATC.Siemens.Model"
             d:DataContext="{d:DesignInstance model:MenuViewModel}"
             d:DesignHeight="54"
             d:DesignWidth="78"
             mc:Ignorable="d">
    <Grid>
        <Label Margin="5"
               HorizontalContentAlignment="Center"
               VerticalContentAlignment="Center"
               Content="{Binding Path=MenuContent}"
               Foreground="{Binding Path=TextColor}" />
    </Grid>
</UserControl>