<UserControl x:Class="Urban.ATC.CommonView.WPFControl.DomainControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:RegionA="clr-namespace:Urban.ATC.CommonView.WPFControl.RegionA"
             xmlns:RegionB="clr-namespace:Urban.ATC.CommonView.WPFControl.RegionB"
             xmlns:RegionC="clr-namespace:Urban.ATC.CommonView.WPFControl.RegionC"
             xmlns:RegionD="clr-namespace:Urban.ATC.CommonView.WPFControl.RegionD"
             xmlns:RegionF="clr-namespace:Urban.ATC.CommonView.WPFControl.RegionF"
             xmlns:RegionM="clr-namespace:Urban.ATC.CommonView.WPFControl.RegionM"
             xmlns:RegionT="clr-namespace:Urban.ATC.CommonView.WPFControl.RegionT"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:regionE="clr-namespace:Urban.ATC.CommonView.WPFControl.RegionE"
             d:DesignHeight="480"
             d:DesignWidth="640"
             mc:Ignorable="d">
    <Grid Margin="15">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="376*" />
            <ColumnDefinition Width="234*" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="300*" />
            <RowDefinition Height="150*" />
        </Grid.RowDefinitions>
        <Grid Grid.Row="0" Grid.Column="0">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="54*" />
                <ColumnDefinition Width="322*" />
            </Grid.ColumnDefinitions>
            <RegionA:RegionA Grid.Row="0" Grid.Column="0" />
            <RegionB:RegionB Grid.Row="0" Grid.Column="1" />
        </Grid>
        <Grid Grid.Row="0" Grid.Column="1">
            <Grid.RowDefinitions>
                <RowDefinition Height="30*" />
                <RowDefinition Height="54*" />
                <RowDefinition Height="216*" />
            </Grid.RowDefinitions>
            <RegionT:RegionT Grid.Row="0" />
            <Grid Grid.Row="1">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="78*" />
                    <ColumnDefinition Width="78*" />
                    <ColumnDefinition Width="78*" />
                </Grid.ColumnDefinitions>
                <RegionD:RegionD Grid.Column="2" />
            </Grid>
        </Grid>
        <Grid Grid.Row="1" Grid.Column="0">
            <Grid.RowDefinitions>
                <RowDefinition Height="54*" />
                <RowDefinition Height="96*" />
            </Grid.RowDefinitions>
            <RegionC:RegionC Grid.Row="0" Grid.Column="0" />
            <regionE:RegionE Grid.Row="1" Grid.Column="0" />
        </Grid>
        <Grid Grid.Row="1" Grid.Column="1">
            <Grid.RowDefinitions>
                <RowDefinition Height="54*" />
                <RowDefinition Height="96*" />
            </Grid.RowDefinitions>
            <RegionM:RegionMTwo Grid.Row="0" Grid.Column="0" />

            <RegionF:RegionF Grid.Row="1" />
        </Grid>
    </Grid>
</UserControl>