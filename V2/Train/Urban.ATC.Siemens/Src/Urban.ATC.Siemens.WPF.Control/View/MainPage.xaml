<UserControl x:Class="Urban.ATC.Siemens.WPF.Control.View.MainPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:name="clr-namespace:Urban.ATC.Siemens.WPF.Control.Constant"
             xmlns:region="http://www.codeplex.com/prism"
             xmlns:viewModel="clr-namespace:Urban.ATC.Siemens.WPF.Control.ViewModel"
             d:DataContext="{d:DesignInstance viewModel:SiemensData}"
             d:DesignHeight="480"
             d:DesignWidth="640"
             Visibility="{Binding MMIBack}"
             mc:Ignorable="d">
    <Grid x:Name="Grid" Background="Transparent">
        <ContentControl x:Name="MainContent" region:RegionManager.RegionName="{x:Static name:RegionNames.MainRoot}" />
    </Grid>
</UserControl>