<UserControl
    x:Class="Subway.ShenZhenLine7.Views.SubSystem.BrakeView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:car="clr-namespace:Subway.ShenZhenLine7.Views.Car"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:viewModels="clr-namespace:Subway.ShenZhenLine7.ViewModels"
    d:DataContext="{d:DesignInstance viewModels:ShenZhenLine7ViewModel}"
    d:DesignHeight="115"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="52*" />
            <ColumnDefinition Width="18*" />
            <ColumnDefinition Width="110*" />
            <ColumnDefinition Width="110*" />
            <ColumnDefinition Width="110*" />
            <ColumnDefinition Width="110*" />
            <ColumnDefinition Width="110*" />
            <ColumnDefinition Width="110*" />
            <ColumnDefinition Width="18*" />
            <ColumnDefinition Width="52*" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="32*" />
            <RowDefinition Height="16*" />
            <RowDefinition Height="26*" />
            <RowDefinition Height="26*" />
            <RowDefinition Height="16*" />
        </Grid.RowDefinitions>
        <car:CommonCar
            Grid.Row="0"
            Grid.RowSpan="5"
            Grid.Column="0"
            Grid.ColumnSpan="10"
            Panel.ZIndex="-1" />
        <ItemsControl
            Grid.Row="2"
            Grid.Column="2"
            ItemsSource="{Binding MasterPageViewModel.BrakeViewModel.Car1Top}"
            Style="{StaticResource Subway.ShenZhenLine7.BrakeTopStyle}" />
        <ItemsControl
            Grid.Row="2"
            Grid.Column="3"
            ItemsSource="{Binding MasterPageViewModel.BrakeViewModel.Car2Top}"
            Style="{StaticResource Subway.ShenZhenLine7.BrakeTopStyle}" />
        <ItemsControl
            Grid.Row="2"
            Grid.Column="4"
            ItemsSource="{Binding MasterPageViewModel.BrakeViewModel.Car3Top}"
            Style="{StaticResource Subway.ShenZhenLine7.BrakeTopStyle}" />
        <ItemsControl
            Grid.Row="2"
            Grid.Column="5"
            ItemsSource="{Binding MasterPageViewModel.BrakeViewModel.Car4Top}"
            Style="{StaticResource Subway.ShenZhenLine7.BrakeTopStyle}" />
        <ItemsControl
            Grid.Row="2"
            Grid.Column="6"
            ItemsSource="{Binding MasterPageViewModel.BrakeViewModel.Car5Top}"
            Style="{StaticResource Subway.ShenZhenLine7.BrakeTopStyle}" />
        <ItemsControl
            Grid.Row="2"
            Grid.Column="7"
            ItemsSource="{Binding MasterPageViewModel.BrakeViewModel.Car6Top}"
            Style="{StaticResource Subway.ShenZhenLine7.BrakeTopStyle}" />
        <ItemsControl
            Grid.Row="3"
            Grid.Column="2"
            ItemsSource="{Binding MasterPageViewModel.BrakeViewModel.Car1Buttom}"
            Style="{StaticResource Subway.ShenZhenLine7.BrakeTopStyle}" />
        <ItemsControl
            Grid.Row="3"
            Grid.Column="3"
            ItemsSource="{Binding MasterPageViewModel.BrakeViewModel.Car2Buttom}"
            Style="{StaticResource Subway.ShenZhenLine7.BrakeTopStyle}" />
        <ItemsControl
            Grid.Row="3"
            Grid.Column="4"
            ItemsSource="{Binding MasterPageViewModel.BrakeViewModel.Car3Buttom}"
            Style="{StaticResource Subway.ShenZhenLine7.BrakeTopStyle}" />
        <ItemsControl
            Grid.Row="3"
            Grid.Column="5"
            ItemsSource="{Binding MasterPageViewModel.BrakeViewModel.Car4Buttom}"
            Style="{StaticResource Subway.ShenZhenLine7.BrakeTopStyle}" />
        <ItemsControl
            Grid.Row="3"
            Grid.Column="6"
            ItemsSource="{Binding MasterPageViewModel.BrakeViewModel.Car5Buttom}"
            Style="{StaticResource Subway.ShenZhenLine7.BrakeTopStyle}" />
        <ItemsControl
            Grid.Row="3"
            Grid.Column="7"
            ItemsSource="{Binding MasterPageViewModel.BrakeViewModel.Car6Buttom}"
            Style="{StaticResource Subway.ShenZhenLine7.BrakeTopStyle}" />

    </Grid>
</UserControl>
