<UserControl
    x:Class="Motor.TCMS.CRH400BF.View.Contents.Tow.TowLayout"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:constant="clr-namespace:Motor.TCMS.CRH400BF.Constant"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Motor.TCMS.CRH400BF.View.Contents.Tow"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:regions="http://www.codeplex.com/CompositeWPF"
    xmlns:viewModel="clr-namespace:Motor.TCMS.CRH400BF.ViewModel"
    Width="800"
    Height="400"
    d:DataContext="{d:DesignInstance viewModel:DomainViewModel}"
    mc:Ignorable="d">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition Height="4*" />
        </Grid.RowDefinitions>
        <Border regions:RegionManager.RegionName="{x:Static constant:RegionNames.ContentTowTitle}" />
        <Border Grid.Row="1" regions:RegionManager.RegionName="{x:Static constant:RegionNames.ContentTowContent}" />
    </Grid>
</UserControl>
