<UserControl x:Class="Motor.ATP._300T.Subsys.WPFView.RegionE.MessageArrawView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:interface="clr-namespace:Motor.ATP.Infrasturcture.Interface;assembly=Motor.ATP.Infrasturcture"
             xmlns:local="clr-namespace:Motor.ATP._300T.Subsys.WPFView.RegionE"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             d:DataContext="{d:DesignInstance interface:IATP}"
             d:DesignHeight="126"
             d:DesignWidth="46"
             mc:Ignorable="d">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="63*" />
            <RowDefinition Height="63*" />
        </Grid.RowDefinitions>
        <Image Source="{Binding Path=Message.CanNavigateUp, Converter={StaticResource Motor.ATP.300T.BoolToMsgArrawUpConverter}}" Visibility="{Binding Path=Message.CanNavigateUpVisible, Converter={StaticResource Motor.ATP.300T.BoolVisibilityConverter}}" />
        <Image Grid.Row="1"
               Source="{Binding Path=Message.CanNavigateDown,
                                Converter={StaticResource Motor.ATP.300T.BoolToMsgArrawDownConverter}}"
               Visibility="{Binding Path=Message.CanNavigateDownVisible,
                                    Converter={StaticResource Motor.ATP.300T.BoolVisibilityConverter}}" />
    </Grid>
</UserControl>
