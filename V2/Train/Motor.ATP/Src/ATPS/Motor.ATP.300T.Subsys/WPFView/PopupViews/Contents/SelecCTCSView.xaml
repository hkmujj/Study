<UserControl x:Class="Motor.ATP._300T.Subsys.WPFView.PopupViews.Contents.SelecCTCSView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Motor.ATP._300T.Subsys.WPFView.PopupViews.Contents"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             Background="{StaticResource Motor.ATP.300T.PopviewBackgroudBrush}"
             d:DesignHeight="250"
             d:DesignWidth="232"
             mc:Ignorable="d">
    <Grid Margin="50">
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
        </Grid.RowDefinitions>

        <TextBlock Grid.Row="0"
                   Style="{StaticResource Motor.ATP.300T.PopupViewContentTextStyle}"
                   Text="{StaticResource Motor.ATP.Infrasturcture.String.300TCTCS3Level}" />

        <TextBlock Grid.Row="1"
                   Style="{StaticResource Motor.ATP.300T.PopupViewContentTextStyle}"
                   Text="{StaticResource Motor.ATP.Infrasturcture.String.300TCTCS2Level}" />
    </Grid>
</UserControl>
