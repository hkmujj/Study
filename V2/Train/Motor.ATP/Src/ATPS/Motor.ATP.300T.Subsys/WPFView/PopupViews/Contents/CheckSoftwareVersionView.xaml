<UserControl x:Class="Motor.ATP._300T.Subsys.WPFView.PopupViews.Contents.CheckSoftwareVersionView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Motor.ATP._300T.Subsys.WPFView.PopupViews.Contents"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:popupViewModels="clr-namespace:Motor.ATP._300T.Subsys.ViewModel.PopupViewModels"
             Background="{StaticResource Motor.ATP.300T.PopviewBackgroudBrush}"
             d:DesignHeight="250"
             d:DesignWidth="232"
             mc:Ignorable="d">
    <Grid Margin="15,20,25,50">
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
        </Grid.RowDefinitions>
        <TextBlock Style="{StaticResource Motor.ATP.300T.PopupViewContentTextStyle}" Text="{StaticResource Motor.ATP.Infrasturcture.String.DMIVersionInfo}" />
        <TextBlock Grid.Row="1"
                   Style="{StaticResource Motor.ATP.300T.PopupViewContentTextStyle}"
                   Text="{StaticResource Motor.ATP.Infrasturcture.String.DMISoftwareVersion}" />
        <TextBlock Grid.Row="3"
                   Style="{StaticResource Motor.ATP.300T.PopupViewContentTextStyle}"
                   Text="{StaticResource Motor.ATP.Infrasturcture.String.C2CUVersionInfo}" />
        <TextBlock Grid.Row="4"
                   Style="{StaticResource Motor.ATP.300T.PopupViewContentTextStyle}"
                   Text="{StaticResource Motor.ATP.Infrasturcture.String.C2CUSoftwareVersion}" />
        <TextBlock Grid.Row="6"
                   Style="{StaticResource Motor.ATP.300T.PopupViewContentTextStyle}"
                   Text="{StaticResource Motor.ATP.Infrasturcture.String.ATPCUVersionInfo}" />
        <TextBlock Grid.Row="7"
                   Style="{StaticResource Motor.ATP.300T.PopupViewContentTextStyle}"
                   Text="{StaticResource Motor.ATP.Infrasturcture.String.ATPCUSoftwareVersion}" />
    </Grid>
</UserControl>
