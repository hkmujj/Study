<UserControl x:Class="Motor.ATP._300S.Subsys.WPFView.RegionE.ZoomInView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:interface="clr-namespace:Motor.ATP.Infrasturcture.Interface;assembly=Motor.ATP.Infrasturcture"
             xmlns:local="clr-namespace:Motor.ATP._300S.Subsys.WPFView.RegionE"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             d:DataContext="{d:DesignInstance interface:IATP}"
             d:DesignHeight="300"
             d:DesignWidth="300"
             mc:Ignorable="d">
    <Image Source="{Binding Path=SpeedMonitoringSection.PlanSectionCoordinate.CanZoomIn, Converter={StaticResource Motor.ATP.300S.CanZoomInToImageConverter}}" Visibility="{Binding Path=SpeedMonitoringSection.ZoomVisible, Converter={StaticResource Motor.ATP.300S.BoolVisibilityConverter}}" />
</UserControl>
