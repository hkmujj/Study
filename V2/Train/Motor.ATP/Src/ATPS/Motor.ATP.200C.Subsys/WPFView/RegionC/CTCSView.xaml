<UserControl x:Class="Motor.ATP._200C.Subsys.WPFView.RegionC.CTCSView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:interface="clr-namespace:Motor.ATP.Infrasturcture.Interface;assembly=Motor.ATP.Infrasturcture"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             d:DataContext="{d:DesignInstance interface:IATP}"
             d:DesignHeight="27"
             d:DesignWidth="54"
             mc:Ignorable="d">
    <Grid>
        <TextBlock Style="{StaticResource Motor.ATP.200C.CTCSTextStyle}" Text="{Binding Path=CTCS.CurrentCTCSType, Converter={StaticResource Motor.ATP.200C.CtcsTypeToTextConverter}}" />
    </Grid>
</UserControl>
