<UserControl x:Class="Motor.ATP._200C.Subsys.WPFView.PopupViews.Contents.SelectOtherView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             d:DesignHeight="250"
             d:DesignWidth="232"
             Background="{StaticResource Motor.ATP.200C.Bursh.PopViewContent}"
             mc:Ignorable="d">
    <Grid Margin="20,10,50,90">
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
        </Grid.RowDefinitions>
        <TextBlock Grid.Row="0"
                   Style="{StaticResource Motor.ATP.200C.PopupViewContentTextStyle}"
                   Text="{StaticResource Motor.ATP.Infrasturcture.String.Volume}" />
        <TextBlock Grid.Row="1"
                   Style="{StaticResource Motor.ATP.200C.PopupViewContentTextStyle}"
                   Text="{StaticResource Motor.ATP.Infrasturcture.String.Light}" />
        <TextBlock Grid.Row="2"
                   Style="{StaticResource Motor.ATP.200C.PopupViewContentTextStyle}"
                   Text="{StaticResource Motor.ATP.Infrasturcture.String.SoftwareVersion}" />
        <TextBlock Grid.Row="3"
                   Style="{StaticResource Motor.ATP.200C.PopupViewContentTextStyle}"
                   Text="{StaticResource Motor.ATP.Infrasturcture.String.BrakeTest}" />        
        <TextBlock Grid.Row="4"
                   Style="{StaticResource Motor.ATP.200C.PopupViewContentTextStyle}"
                   Text="{StaticResource Motor.ATP.Infrasturcture.String.DMITest}" />
    </Grid>
</UserControl>
