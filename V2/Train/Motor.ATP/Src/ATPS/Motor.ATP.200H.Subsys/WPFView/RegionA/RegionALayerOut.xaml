<UserControl x:Class="Motor.ATP._200H.Subsys.WPFView.RegionA.RegionALayerout"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:regions="http://www.codeplex.com/CompositeWPF"
             xmlns:constant="clr-namespace:Motor.ATP._200H.Subsys.Constant"
             d:DesignHeight="300"
             d:DesignWidth="54"
             mc:Ignorable="d">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="54*" />
            <RowDefinition Height="222*" />
            <RowDefinition Height="24*" />

        </Grid.RowDefinitions>

        <Border Style="{StaticResource Motor.ATP.200H.BackGridBorderStyleLeftUp}" regions:RegionManager.RegionName="{x:Static constant:RegionNames.RegionA01}" />
        <Border Grid.Row="1"
                Style="{StaticResource Motor.ATP.200H.BackGridBorderStyleLeftUp}"
                regions:RegionManager.RegionName="{x:Static constant:RegionNames.RegionA02}" />
        <Border Grid.Row="2"
                Style="{StaticResource Motor.ATP.200H.BackGridBorderStyleLeftUp}"
                regions:RegionManager.RegionName="{x:Static constant:RegionNames.RegionA03}" />
    </Grid>
</UserControl>
