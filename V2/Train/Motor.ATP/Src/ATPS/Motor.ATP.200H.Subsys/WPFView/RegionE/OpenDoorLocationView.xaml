<UserControl x:Class="Motor.ATP._200H.Subsys.WPFView.RegionE.OpenDoorLocationView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:interface="clr-namespace:Motor.ATP.Infrasturcture.Interface;assembly=Motor.ATP.Infrasturcture"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             d:DataContext="{d:DesignInstance interface:IATP}"
             d:DesignHeight="49"
             d:DesignWidth="49"
             mc:Ignorable="d">
    <Image Source="{Binding Path=TrainInfo.Station.CurrentOpenDoorLocation, Converter={StaticResource Motor.ATP.200H.OpenDoorLocationToImageConverter}}" Stretch="Fill" />
</UserControl>
