<UserControl x:Class="Motor.ATP._200H.Subsys.WPFView.PopupViews.Contents.SelectControlModelView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:popupViewModels="clr-namespace:Motor.ATP._200H.Subsys.ViewModel.PopupViewModels"
             d:DataContext="{d:DesignInstance popupViewModels:SelectControlModelPopupViewModel}"
             d:DesignHeight="250"
             d:DesignWidth="232"
             Background="{StaticResource Motor.ATP.200H.Bursh.PopViewContent}"
             mc:Ignorable="d">
    <StackPanel Margin="10,70,10,20" Orientation="Vertical">
        <TextBlock Margin="10"
                   Style="{StaticResource Motor.ATP.200H.PopupViewCenterContentTextStyle}"
                   Text="{StaticResource Motor.ATP.Infrasturcture.String.ControlTypeShuntingModel}" />
        <TextBlock Margin="10,30"
                   Style="{StaticResource Motor.ATP.200H.PopupViewCenterContentTextStyle}"
                   Text="{StaticResource Motor.ATP.Infrasturcture.String.ControlTypeOnSightModel}" />
    </StackPanel>
</UserControl>
