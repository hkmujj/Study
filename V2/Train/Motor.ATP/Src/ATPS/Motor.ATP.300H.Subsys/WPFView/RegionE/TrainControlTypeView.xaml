<UserControl x:Class="Motor.ATP._300H.Subsys.WPFView.RegionE.TrainControlTypeView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:interface="clr-namespace:Motor.ATP.Infrasturcture.Interface;assembly=Motor.ATP.Infrasturcture"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             d:DataContext="{d:DesignInstance interface:IATP}"
             d:DesignHeight="30"
             d:DesignWidth="54"
             mc:Ignorable="d">
    <Border>
        <TextBlock Style="{StaticResource Motor.ATP.300H.TrainControlTypeTextStyle}" Text="{Binding Path=TrainControlType, Converter={StaticResource Motor.ATP.300H.TrainControlTypeToStringConverter}}" />
    </Border>
</UserControl>
