<UserControl x:Class="Subway.WuHanLine6.Views.TitleContentShell.RunTitleContentShell"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:constance="clr-namespace:Subway.WuHanLine6.Constance"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Subway.WuHanLine6.Views.TitleContentShell"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:regions="http://www.codeplex.com/CompositeWPF"
             d:DesignHeight="555"
             d:DesignWidth="800"
             mc:Ignorable="d">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="60*" />
            <RowDefinition Height="495*" />
        </Grid.RowDefinitions>
        <ContentControl regions:RegionManager.RegionName="{x:Static constance:RegionNames.TitleRegion}" />
        <ContentControl Grid.Row="1" regions:RegionManager.RegionName="{x:Static constance:RegionNames.MainContent}" />
    </Grid>
</UserControl>