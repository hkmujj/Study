<UserControl x:Class="Subway.WuHanLine6.Views.HelpContent.RunHelp.RunHelpPageThree"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:view="clr-namespace:MMI.Facility.WPFInfrastructure.View;assembly=MMI.Facility.WPFInfrastructure"
             xmlns:viewModels="clr-namespace:Subway.WuHanLine6.ViewModels"
             d:DataContext="{d:DesignInstance viewModels:WuHanViewModel}"
             d:DesignHeight="465"
             d:DesignWidth="800"
             mc:Ignorable="d">
    <Grid>
        <Grid Margin="40,10,10,40"
              view:GridLine.Brush="DarkGray"
              view:GridLine.Style="ShowIfHasControl"
              view:GridLine.Width="2"
              view:GridLine.WidthStyle="AllLineSame">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="55*" />
                <ColumnDefinition Width="40*" />
                <ColumnDefinition Width="55*" />
                <ColumnDefinition Width="150*" />
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="1*" />
                <RowDefinition Height="1*" />
                <RowDefinition Height="1*" />
                <RowDefinition Height="1*" />
                <RowDefinition Height="1*" />
                <RowDefinition Height="1*" />
                <RowDefinition Height="1*" />
                <RowDefinition Height="1*" />
                <RowDefinition Height="1*" />
                <RowDefinition Height="1*" />
                <RowDefinition Height="1*" />
                <RowDefinition Height="1*" />
                <RowDefinition Height="1*" />
                <RowDefinition Height="1*" />
                <RowDefinition Height="1*" />
            </Grid.RowDefinitions>

            <TextBlock Grid.Row="0"
                       Grid.Column="0"
                       Style="{StaticResource HelpViewTextStyle}"
                       Text="显示项目" />
            <TextBlock Grid.Row="0"
                       Grid.Column="1"
                       Style="{StaticResource HelpViewTextStyle}"
                       Text="显示条件" />
            <TextBlock Grid.Row="0"
                       Grid.Column="2"
                       Style="{StaticResource HelpViewTextStyle}"
                       Text="显示" />
            <TextBlock Grid.Row="0"
                       Grid.Column="3"
                       Style="{StaticResource HelpViewTextStyle}"
                       Text="备注" />
            <TextBlock Grid.Row="1"
                       Grid.RowSpan="7"
                       Grid.Column="0"
                       Style="{StaticResource HelpViewTextStyle}"
                       Text="制动系统状态" />
            <TextBlock Grid.Row="1"
                       Grid.Column="1"
                       Style="{StaticResource HelpViewTextStyle}"
                       Text="停放制动施加" />

            <Image Grid.Row="1"
                   Grid.Column="2"
                   Source="{StaticResource 停放制动施加}" />
            <Grid Grid.Row="1" Grid.Column="3" />
            <TextBlock Grid.Row="2"
                       Grid.Column="1"
                       Style="{StaticResource HelpViewTextStyle}"
                       Text="制动切除" />
            <Image Grid.Row="2"
                   Grid.Column="2"
                   Source="{StaticResource 制动切除}" />
            <Grid Grid.Row="2" Grid.Column="3" />
            <TextBlock Grid.Row="3"
                       Grid.Column="1"
                       Style="{StaticResource HelpViewTextStyle}"
                       Text="制动自检激活" />
            <Image Grid.Row="3"
                   Grid.Column="2"
                   Source="{StaticResource 制动自检Img}" />
            <Grid Grid.Row="3" Grid.Column="3" />
            <TextBlock Grid.Row="4"
                       Grid.Column="1"
                       Style="{StaticResource HelpViewTextStyle}"
                       Text="制动故障" />
            <Image Grid.Row="4"
                   Grid.Column="2"
                   Source="{StaticResource 制动故障}" />
            <Grid Grid.Row="4" Grid.Column="3" />
            <TextBlock Grid.Row="5"
                       Grid.Column="1"
                       Style="{StaticResource HelpViewTextStyle}"
                       Text="制动警告" />
            <Image Grid.Row="5"
                   Grid.Column="2"
                   Source="{StaticResource 制动警告}" />
            <Grid Grid.Row="5" Grid.Column="3" />
            <TextBlock Grid.Row="6"
                       Grid.Column="1"
                       Style="{StaticResource HelpViewTextStyle}"
                       Text="常用制动施加" />
            <Image Grid.Row="6"
                   Grid.Column="2"
                   Source="{StaticResource 常用制动施加}" />
            <Grid Grid.Row="6" Grid.Column="3" />
            <TextBlock Grid.Row="7"
                       Grid.Column="1"
                       Style="{StaticResource HelpViewTextStyle}"
                       Text="常用制动缓解" />
            <Image Grid.Row="7"
                   Grid.Column="2"
                   Source="{StaticResource 常用制动缓解}" />
            <Grid Grid.Row="7" Grid.Column="3" />
            <TextBlock Grid.Row="8"
                       Grid.RowSpan="7"
                       Grid.Column="0"
                       Style="{StaticResource HelpViewTextStyle}"
                       Text="空调系统状态" />
            <TextBlock Grid.Row="8"
                       Grid.Column="1"
                       Style="{StaticResource HelpViewTextStyle}"
                       Text="空调故障" />
            <Image Grid.Row="8"
                   Grid.Column="2"
                   Source="{StaticResource 空调故障}" />
            <Grid Grid.Row="8" Grid.Column="3" />
            <TextBlock Grid.Row="9"
                       Grid.Column="1"
                       Style="{StaticResource HelpViewTextStyle}"
                       Text="空调警告" />
            <Image Grid.Row="9"
                   Grid.Column="2"
                   Source="{StaticResource 空调警告}" />
            <Grid Grid.Row="9" Grid.Column="3" />
            <TextBlock Grid.Row="10"
                       Grid.Column="1"
                       Style="{StaticResource HelpViewTextStyle}"
                       Text="紧急通风" />
            <Image Grid.Row="10"
                   Grid.Column="2"
                   Source="{StaticResource 紧急通风}" />
            <Grid Grid.Row="10" Grid.Column="3" />
            <TextBlock Grid.Row="11"
                       Grid.Column="1"
                       Style="{StaticResource HelpViewTextStyle}"
                       Text="通风" />
            <Image Grid.Row="11"
                   Grid.Column="2"
                   Source="{StaticResource 通风}" />
            <Grid Grid.Row="11" Grid.Column="3" />
            <TextBlock Grid.Row="12"
                       Grid.Column="1"
                       Style="{StaticResource HelpViewTextStyle}"
                       Text="限制制冷" />
            <Image Grid.Row="12"
                   Grid.Column="2"
                   Source="{StaticResource 限制制冷}" />
            <Grid Grid.Row="12" Grid.Column="3" />
            <TextBlock Grid.Row="13"
                       Grid.Column="1"
                       Style="{StaticResource HelpViewTextStyle}"
                       Text="空调运行" />
            <Image Grid.Row="13"
                   Grid.Column="2"
                   Source="{StaticResource 空调运行}" />
            <Grid Grid.Row="13" Grid.Column="3" />
            <TextBlock Grid.Row="14"
                       Grid.Column="1"
                       Style="{StaticResource HelpViewTextStyle}"
                       Text="空调断开" />
            <Image Grid.Row="14"
                   Grid.Column="2"
                   Source="{StaticResource 空调断开}" />
            <Grid Grid.Row="14" Grid.Column="3" />
        </Grid>
    </Grid>
</UserControl>