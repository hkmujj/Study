<UserControl x:Class="Subway.WuHanLine6.Views.HelpContent.RunHelp.RunHelpPageFive"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Subway.WuHanLine6.Views.HelpContent.RunHelp"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:view="clr-namespace:MMI.Facility.WPFInfrastructure.View;assembly=MMI.Facility.WPFInfrastructure"
             xmlns:viewModels="clr-namespace:Subway.WuHanLine6.ViewModels"
             d:DataContext="{d:DesignInstance viewModels:WuHanViewModel}"
             d:DesignHeight="465"
             d:DesignWidth="800"
             mc:Ignorable="d">
    <Grid>
        <Grid Margin="40,10,10,70"
              view:GridLine.Brush="DarkGray"
              view:GridLine.Style="ShowIfHasControl"
              view:GridLine.Width="2"
              view:GridLine.WidthStyle="AllLineSame">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="55*" />
                <ColumnDefinition Width="40*" />
                <ColumnDefinition Width="55*" />
                <ColumnDefinition Width="150*" />
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="1*" />
                <RowDefinition Height="1*" />
                <RowDefinition Height="1*" />
                <RowDefinition Height="1*" />
                <RowDefinition Height="1*" />
                <RowDefinition Height="1*" />
                <RowDefinition Height="1*" />
                <RowDefinition Height="1*" />
                <RowDefinition Height="1*" />
                <RowDefinition Height="1*" />
                <RowDefinition Height="1*" />
                <RowDefinition Height="1*" />
                <RowDefinition Height="1*" />
            </Grid.RowDefinitions>
            <TextBlock Grid.Row="0"
                       Grid.Column="0"
                       Style="{StaticResource HelpViewTextStyle}"
                       Text="显示项目" />
            <TextBlock Grid.Row="0"
                       Grid.Column="1"
                       Style="{StaticResource HelpViewTextStyle}"
                       Text="显示条件" />
            <TextBlock Grid.Row="0"
                       Grid.Column="2"
                       Style="{StaticResource HelpViewTextStyle}"
                       Text="显示" />
            <TextBlock Grid.Row="0"
                       Grid.Column="3"
                       Style="{StaticResource HelpViewTextStyle}"
                       Text="备注" />
            <TextBlock Grid.Row="1"
                       Grid.RowSpan="5"
                       Grid.Column="0"
                       Style="{StaticResource HelpViewTextStyle}"
                       Text="空压机状态" />
            <TextBlock Grid.Row="1"
                       Grid.Column="1"
                       Style="{StaticResource HelpViewTextStyle}"
                       Text="切除" />
            <Image Grid.Row="1"
                   Grid.Column="2"
                   Source="{StaticResource 辅助系统切除}" />
            <TextBlock Grid.Row="1"
                       Grid.Column="3"
                       Style="{StaticResource HelpViewTextStyle}"
                       Text="" />
            <TextBlock Grid.Row="2"
                       Grid.Column="1"
                       Style="{StaticResource HelpViewTextStyle}"
                       Text="故障" />
            <Image Grid.Row="2"
                   Grid.Column="2"
                   Source="{StaticResource 空压机故障}" />
            <TextBlock Grid.Row="2"
                       Grid.Column="3"
                       Style="{StaticResource HelpViewTextStyle}"
                       Text="" />
            <TextBlock Grid.Row="3"
                       Grid.Column="1"
                       Style="{StaticResource HelpViewTextStyle}"
                       Text="警告" />
            <Image Grid.Row="3"
                   Grid.Column="2"
                   Source="{StaticResource 空压机警告}" />
            <TextBlock Grid.Row="3"
                       Grid.Column="3"
                       Style="{StaticResource HelpViewTextStyle}"
                       Text="" />
            <TextBlock Grid.Row="4"
                       Grid.Column="1"
                       Style="{StaticResource HelpViewTextStyle}"
                       Text="运行" />
            <Image Grid.Row="4"
                   Grid.Column="2"
                   Source="{StaticResource 空压机运行}" />
            <TextBlock Grid.Row="4"
                       Grid.Column="3"
                       Style="{StaticResource HelpViewTextStyle}"
                       Text="" />
            <TextBlock Grid.Row="5"
                       Grid.Column="1"
                       Style="{StaticResource HelpViewTextStyle}"
                       Text="断开" />
            <Image Grid.Row="5"
                   Grid.Column="2"
                   Source="{StaticResource 空压机断开}" />
            <TextBlock Grid.Row="5"
                       Grid.Column="3"
                       Style="{StaticResource HelpViewTextStyle}"
                       Text="" />
            <TextBlock Grid.Row="6"
                       Grid.RowSpan="7"
                       Grid.Column="0"
                       Style="{StaticResource HelpViewTextStyle}"
                       Text="运行模式" />
            <TextBlock Grid.Row="6"
                       Grid.Column="1"
                       Style="{StaticResource HelpViewTextStyle}"
                       Text="ATC通信异常" />
            <Border Grid.Row="6"
                    Grid.Column="2"
                    Margin="20,1"
                    Background="Red">
                <TextBlock FontSize="14"
                           Foreground="Black"
                           Style="{StaticResource HelpViewTextStyle}"
                           Text="ATC&#13;通信异常" />
            </Border>
            <TextBlock Grid.Row="6"
                       Grid.Column="3"
                       Style="{StaticResource HelpViewTextStyle}"
                       Text="优先级由上到下依次降低" />
            <TextBlock Grid.Row="7"
                       Grid.Column="1"
                       Style="{StaticResource HelpViewTextStyle}"
                       Text="ATC无效" />
            <Border Grid.Row="7"
                    Grid.Column="2"
                    Margin="20,1"
                    Background="Yellow">
                <TextBlock FontSize="14"
                           Foreground="Black"
                           Style="{StaticResource HelpViewTextStyle}"
                           Text="ATC无效" />
            </Border>
            <TextBlock Grid.Row="7"
                       Grid.Column="3"
                       Style="{StaticResource HelpViewTextStyle}"
                       Text="" />
            <TextBlock Grid.Row="8"
                       Grid.Column="1"
                       Style="{StaticResource HelpViewTextStyle}"
                       Text="手动模式" />
            <Border Grid.Row="8"
                    Grid.Column="2"
                    Margin="20,1"
                    Background="#FF00FF00">
                <TextBlock FontSize="14"
                           Foreground="Black"
                           Style="{StaticResource HelpViewTextStyle}"
                           Text="手动模式" />
            </Border>
            <TextBlock Grid.Row="8"
                       Grid.Column="3"
                       Style="{StaticResource HelpViewTextStyle}"
                       Text="" />
            <TextBlock Grid.Row="9"
                       Grid.Column="1"
                       Style="{StaticResource HelpViewTextStyle}"
                       Text="人工向前" />
            <Border Grid.Row="9"
                    Grid.Column="2"
                    Margin="20,1"
                    Background="#FF00FF00">
                <TextBlock FontSize="14"
                           Foreground="Black"
                           Style="{StaticResource HelpViewTextStyle}"
                           Text="人工向前" />
            </Border>
            <TextBlock Grid.Row="9"
                       Grid.Column="3"
                       Style="{StaticResource HelpViewTextStyle}"
                       Text="" />
            <TextBlock Grid.Row="10"
                       Grid.Column="1"
                       Style="{StaticResource HelpViewTextStyle}"
                       Text="人工向后" />
            <Border Grid.Row="10"
                    Grid.Column="2"
                    Margin="20,1"
                    Background="#FF00FF00">
                <TextBlock FontSize="14"
                           Foreground="Black"
                           Style="{StaticResource HelpViewTextStyle}"
                           Text="人工向后" />
            </Border>
            <TextBlock Grid.Row="10"
                       Grid.Column="3"
                       Style="{StaticResource HelpViewTextStyle}"
                       Text="" />
            <TextBlock Grid.Row="11"
                       Grid.Column="1"
                       Style="{StaticResource HelpViewTextStyle}"
                       Text="ATO模式" />
            <Border Grid.Row="11"
                    Grid.Column="2"
                    Margin="20,1"
                    Background="#FF00FF00">
                <TextBlock FontSize="14"
                           Foreground="Black"
                           Style="{StaticResource HelpViewTextStyle}"
                           Text="AT0模式" />
            </Border>
            <TextBlock Grid.Row="11"
                       Grid.Column="3"
                       Style="{StaticResource HelpViewTextStyle}"
                       Text="" />
            <TextBlock Grid.Row="12"
                       Grid.Column="1"
                       Style="{StaticResource HelpViewTextStyle}"
                       Text="ATB模式" />
            <Border Grid.Row="12"
                    Grid.Column="2"
                    Margin="20,1"
                    Background="#FF00FF00">
                <TextBlock FontSize="14"
                           Foreground="Black"
                           Style="{StaticResource HelpViewTextStyle}"
                           Text="ATB模式" />
            </Border>
            <TextBlock Grid.Row="12"
                       Grid.Column="3"
                       Style="{StaticResource HelpViewTextStyle}"
                       Text="" />
        </Grid>
    </Grid>
</UserControl>