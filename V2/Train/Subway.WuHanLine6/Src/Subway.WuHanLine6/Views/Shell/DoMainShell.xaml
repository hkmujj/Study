<UserControl x:Class="Subway.WuHanLine6.Views.Shell.DoMainShell"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Subway.WuHanLine6.Views.Shell"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:name="clr-namespace:Subway.WuHanLine6.Constance"
             xmlns:regions="http://www.codeplex.com/CompositeWPF"
             d:DesignHeight="600"
             d:DesignWidth="800"
             Background="{DynamicResource BackgroundBrush}"
             mc:Ignorable="d">
    <Grid Margin="10">
        <Viewbox Stretch="Fill">
            <ContentControl Width="800"
                            Height="600"
                            regions:RegionManager.RegionName="{x:Static name:RegionNames.MainRoot}"
                            Background="{DynamicResource BackgroundBrush}" />
        </Viewbox>
    </Grid>
</UserControl>