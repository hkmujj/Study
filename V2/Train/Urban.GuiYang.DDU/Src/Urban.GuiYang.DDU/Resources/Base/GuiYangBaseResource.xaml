    <SolidColorBrush x:Key="Blue2Brush" Color="#01749A" />

    <SolidColorBrush x:Key="Gay1Brush" Color="#939393" />

    <SolidColorBrush x:Key="Gay2Brush" Color="#D7D7D7" />

    <FontFamily x:Key="FontFamilyArial">Arial</FontFamily>

    <FontFamily x:Key="FontFamilySongTypeface">宋体</FontFamily>

    <system:Boolean x:Key="BooleanTrue">True</system:Boolean>
    <system:Boolean x:Key="BooleanFalse">False</system:Boolean>

