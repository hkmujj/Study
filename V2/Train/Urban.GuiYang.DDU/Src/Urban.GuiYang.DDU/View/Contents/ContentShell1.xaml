<UserControl
    x:Class="Urban.GuiYang.DDU.View.Contents.ContentShell1"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:constant="clr-namespace:Urban.GuiYang.DDU.Constant"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Urban.GuiYang.DDU.View.Contents"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:regions="http://www.codeplex.com/CompositeWPF"
    d:DesignHeight="410"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="92*" />
            <RowDefinition Height="133*" />
        </Grid.RowDefinitions>
        <Border regions:RegionManager.RegionName="{x:Static constant:RegionNames.ContentContentTrain}" />
        <Viewbox Grid.Row="1" Stretch="Fill">
            <Border
                Width="800"
                Height="266"
                regions:RegionManager.RegionName="{x:Static constant:RegionNames.ContentContentContent}" />
        </Viewbox>
    </Grid>
</UserControl>
