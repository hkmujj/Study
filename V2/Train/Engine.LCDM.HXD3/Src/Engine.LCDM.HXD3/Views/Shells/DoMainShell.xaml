<UserControl x:Class="Engine.LCDM.HXD3.Views.Shells.DoMainShell"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Engine.LCDM.HXD3.Views.Shells"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             d:DesignHeight="600"
             d:DesignWidth="800"
             xmlns:name="clr-namespace:Engine.LCDM.HXD3.Constance"
             xmlns:region="http://www.codeplex.com/CompositeWPF"
             Background="Black"
             mc:Ignorable="d">
    <Viewbox Stretch="Fill">
        <Grid>
            <ContentControl Width="800" Height="600" region:RegionManager.RegionName="{x:Static name:RegionNames.MainRoot}" />
        </Grid>
    </Viewbox>
   
</UserControl>
